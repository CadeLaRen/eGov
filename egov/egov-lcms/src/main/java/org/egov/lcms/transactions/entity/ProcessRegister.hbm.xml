<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.egov.lcms.transactions.entity.ProcessRegister" table="EGLC_PROCESSREGISTER">
        <id
			name="id"
			column="ID"
			type="long"
			unsaved-value="null">
			<generator class="native">
				<param name="sequence">SEQ_EGLC_PROCESSREGISTER</param>
			</generator>
		</id>
		 <property name="detailedAddress" type="java.lang.String">
            <column name="DETAILED_ADDRESS"/>
        </property>
     
         <property name="processDate" type="java.util.Date">
	    	<column name="PROCESS_DATE"/>
        </property>
        
        <property name="nextDateOfProcess" type="java.util.Date">
	    	<column name="NEXTDATEOFPROCESS"/>
        </property>
        
        <property name="receivedOn" type="java.util.Date">
	    	<column name="RECEIVEDON"/>
        </property>
        
        <property name="processHandedOverTo" type="java.lang.String">
	    	<column name="PROCESSHANDEDOVERTO"/>
        </property>
        
        <property name="dateOfHandingOver" type="java.util.Date">
	    	<column name="DATEOFHANDINGOVER"/>
        </property>
        
        <property name="processFilingAttorney" type="java.lang.String">
	    	<column name="PROCESSFILINGATTORNEY"/>
        </property>
        
        <property name="remarks" type="java.lang.String">
	    	<column name="REMARKS"/>
        </property>
        
        <property name="createdDate" type="java.util.Date">
    		<column name="CREATEDDATE" not-null="true" />
    	</property>
    	
    	<property name="modifiedDate" type="java.util.Date">
            <column name="LASTMODIFIEDDATE" not-null="true" />
        </property>
		
		<many-to-one name="createdBy" class="org.egov.infra.admin.master.entity.User" column="CREATEDBY"  not-null="true" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.infra.admin.master.entity.User" column="lastMODIFIEDBY" not-null="true" fetch="select"/>
		
        <many-to-one name="legalcase" class="org.egov.lcms.transactions.entity.Legalcase" fetch="select">
            <column name="LEGALCASE" precision="22" scale="0" not-null="true" />
        </many-to-one>
        
        <property name="isProcessRegReqd"  type="boolean">
            <column name="ISPROCESSREGREQUIRED"  precision="22" scale="0"/>
        </property>
    </class>
</hibernate-mapping>
