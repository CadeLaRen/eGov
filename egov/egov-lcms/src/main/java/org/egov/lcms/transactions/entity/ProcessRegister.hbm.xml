<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.egov.lcms.models.transactions.ProcessRegister" table="EGLC_PROCESSREGISTER">
        <id
			name="id"
			column="ID"
			type="long"
			unsaved-value="null">
			<generator class="native">
				<param name="sequence">SEQ_EGLC_PROCESSREGISTER</param>
			</generator>
		</id>
		 <property name="detailedAddress" type="java.lang.String">
            <column name="DETAILED_ADDRESS"/>
        </property>
     
         <property name="processDate" type="java.util.Date">
	    	<column name="PROCESS_DATE"/>
        </property>
        
        <property name="nextDateOfProcess" type="java.util.Date">
	    	<column name="NEXT_DATE_OF_PROCESS"/>
        </property>
        
        <property name="receivedOn" type="java.util.Date">
	    	<column name="RECEIVED_ON"/>
        </property>
        
        <property name="processHandedOverTo" type="java.lang.String">
	    	<column name="PROCESS_HANDED_OVER_TO"/>
        </property>
        
        <property name="dateOfHandingOver" type="java.util.Date">
	    	<column name="DATE_OF_HANDING_OVER"/>
        </property>
        
        <property name="processFilingAttorney" type="java.lang.String">
	    	<column name="PROCESS_FILING_ATTORNEY"/>
        </property>
        
        <property name="remarks" type="java.lang.String">
	    	<column name="REMARKS"/>
        </property>
        
        <property name="createdDate" type="java.util.Date">
    		<column name="CREATED_DATE" not-null="true" />
    	</property>
    	
    	<property name="modifiedDate" type="java.util.Date">
            <column name="MODIFIED_DATE" not-null="true" />
        </property>
		
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY"  not-null="true" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" not-null="true" fetch="select"/>
		
        <many-to-one name="legalcase" class="org.egov.lcms.models.transactions.Legalcase" fetch="select">
            <column name="ID_LEGALCASE" precision="22" scale="0" not-null="true" />
        </many-to-one>
        
        <property name="isProcessRegReqd" >
            <column name="IS_PROCESSREGREQUIRED"  precision="22" scale="0"/>
        </property>
    </class>
</hibernate-mapping>
