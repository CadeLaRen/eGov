<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.tl.entity.MotorDetails" table="EGTL_MOTOR_DETAILS">
		<id name="id" type="java.lang.Long">
			<column name="ID" precision="22" scale="0" />
			<generator class="native">
				<param name="sequence">SEQ_EGTL_MOTOR_DETAILS</param>
			</generator>
		</id>
		<many-to-one name="license" class="org.egov.tl.entity.License" fetch="select">
			<column name="ID_LICENSE" precision="22" scale="0" not-null="true" />
		</many-to-one>
		<property name="hp" type="java.math.BigDecimal">
			<column name="HP" precision="22" scale="0"  />
		</property>
		<property name="noOfMachines" type="java.lang.Long">
			<column name="NO_OF_MACHINES" precision="22" scale="0"  />
		</property>
		<property name="history" type="boolean">
			<column name="HISTORY" />
		</property>

	</class>
</hibernate-mapping>
