<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.license.domain.entity.LicenseSubType" table="EGL_MSTR_LICENSE_SUB_TYPE">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native">
				<param name="sequence">SEQ_EGL_MSTR_LICENSE_SUB_TYPE</param>
			</generator>
		</id>
		<property name="name" type="string" column="NAME"/>
		<property name="code" type="string" column="CODE"/>
		 <many-to-one name="licenseType" class="org.egov.license.domain.entity.LicenseType" >
            <column name="ID_LICENSE_TYPE"  not-null="true" />
        </many-to-one>
	</class>
</hibernate-mapping>