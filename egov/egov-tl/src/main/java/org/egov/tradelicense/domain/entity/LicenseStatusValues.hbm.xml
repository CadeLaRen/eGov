<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.license.domain.entity.LicenseStatusValues"
		table="EGL_STATUS_VALUES">
		<id name="id" column="ID_STATUS_VALUES" type="java.lang.Long">
			<generator class="sequence">
				<param name="sequence">SEQ_EGL_STATUS_VALUES</param>
			</generator>
		</id>
		<property name="createdDate" type="java.util.Date">
			<column name="CREATED_DATE" not-null="true" />
		</property>
		<property name="modifiedDate" type="java.util.Date">
			<column name="MODIFIED_DATE" not-null="true" />
		</property>
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl"
			cascade="all">
			<column name="CREATED_BY" />
		</many-to-one>
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl"
			cascade="all">
			<column name="MODIFIED_BY" />
		</many-to-one>

		<many-to-one name="license" column="ID_LICENSE" 
			class="org.egov.license.domain.entity.License" insert="false"
			update="false" />

		<many-to-one name="licenseStatus" column="ID_STATUS"
			class="org.egov.license.domain.entity.LicenseStatus" />

		<property name="reason" column="ID_REASON" />
		<property name="docNumber" column="DOC_NUM" />
		<property name="referenceDate" column="REF_DATE" />
		<property name="referenceNo" column="REF_NUM" />
		<property name="remarks" column="REMARKS" />
		<property name="active" column="IS_ACTIVE" type="yes_no" />
		<property name="extraField1" column="EXTRA_FIELD1" />
		<property name="extraField2" column="EXTRA_FIELD2" />
		<property name="extraField3" column="EXTRA_FIELD3" />
		
<many-to-one name="previousStatusVal"	class="org.egov.license.domain.entity.LicenseStatusValues" column="PARENT_STATUS_VAL"/>

	</class>
</hibernate-mapping>