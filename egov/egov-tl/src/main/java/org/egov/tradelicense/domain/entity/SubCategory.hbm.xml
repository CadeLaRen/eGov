<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">
	<class name="org.egov.license.domain.entity.SubCategory" table="EGL_MSTR_SUB_CATEGORY">
		<id column="ID" name="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">SEQ_EGL_MSTR_SUB_CATEGORY</param>
			</generator>
		</id>
		<property generated="never" lazy="false" name="name" type="java.lang.String">
			<column length="256" name="NAME" not-null="true" />
		</property>
		<property generated="never" lazy="false" name="sectionApplicable" type="java.lang.String">
			<column length="256" name="SECTION_APPLICABLE" />
		</property>
		<many-to-one class="org.egov.license.domain.entity.Schedule" fetch="select" name="scheduleMaster">
			<column name="ID_SCHEDULE" />
		</many-to-one>
		<many-to-one class="org.egov.license.domain.entity.NatureOfBusiness" fetch="select" name="natureOfBusiness">
			<column name="ID_NATURE" />
		</many-to-one>
		<many-to-one class="org.egov.license.domain.entity.LicenseCategory" fetch="select" name="category">
			<column name="ID_CATEGORY" />
		</many-to-one>
		<many-to-one class="org.egov.license.domain.entity.LicenseType" fetch="select" name="licenseType">
			<column name="ID_LICENSE_TYPE" />
		</many-to-one>
		<property generated="never" lazy="false" name="approvalrequired" type="java.lang.Boolean">
			<column name="APPROVAL_REQUIRED" />
		</property>
		<property generated="never" lazy="false" name="pfaApplicable" type="java.lang.Boolean">
			<column name="PFA_APPLICABLE" />
		</property>
		<property generated="never"  name="nocApplicable" type="java.lang.Boolean">
			<column name="NOC_APPLICABLE" />
		</property>
		<property generated="never" lazy="false" name="feeBasedOn" type="java.lang.String">
			<column length="40" name="FEE_BASED_ON"  />
		</property>
		<property generated="never" lazy="false" name="code" type="java.lang.String">
			<column length="32" name="code" not-null="true" unique="true" />
		</property>
		<property generated="never" lazy="false" name="createdDate" type="java.util.Date">
			<column name="CREATEDDATE" not-null="true" />
		</property>
		<property generated="never" lazy="false" name="modifiedDate" type="java.util.Date">
			<column name="LASTMODIFIEDDATE" not-null="true" />
		</property>
		<many-to-one class="org.egov.lib.rjbac.user.UserImpl" column="CREATEDBY" fetch="select" name="createdBy"
			not-null="true" />
		<many-to-one class="org.egov.lib.rjbac.user.UserImpl" column="LASTMODIFIEDBY" fetch="select" name="modifiedBy"
			not-null="true" />
		<many-to-one name="licenseSubType" class="org.egov.license.domain.entity.LicenseSubType"
			cascade="all">
			<column name="ID_LICENSE_SUB_TYPE" />
		</many-to-one>
	</class>
	<query name="SUBCATEGORY_BY_NAME">
		from org.egov.license.domain.entity.SubCategory where name=?
	</query>
</hibernate-mapping>
