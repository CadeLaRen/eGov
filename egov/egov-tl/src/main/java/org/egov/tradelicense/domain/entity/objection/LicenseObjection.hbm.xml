<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
<class name="org.egov.license.domain.entity.objection.LicenseObjection" table="EGL_OBJECTION" >
<id name="id" type="java.lang.Long">
<column name="id"/>
<generator class="native">
<param name="sequence">SEQ_EGL_OBJECTION</param>
</generator>
</id>
<property name="number" column="OBJECTIONNUMBER" type="string"/>
<property name="reason" type="java.lang.Integer"/>
<property name="name" type="string"/>
<property name="address" type="string"/>
<property name="objectionDate" type="date"/>
<property name="details" type="string"/>
<many-to-one name="license" column="ID_LICENSE" class="org.egov.license.domain.entity.License" cascade="all" />
<property name="docNumber" type="string"/>
<many-to-one name="state" class="org.egov.infstr.models.State" cascade="all">
					<column name="STATE_ID" />
</many-to-one>
<property name="createdDate" column="CREATEDDATE" type="timestamp" />
<property name="modifiedDate" column="LASTMODIFIEDDATE" type="timestamp" />
<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATEDBY"  not-null="true" fetch="select" />
<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="LASTMODIFIEDBY" not-null="true" fetch="select"/>
 <bag name="activities" cascade="all,all-delete-orphan" table="EGL_ACTIVITY" inverse="true">
            <key> 
            <column name="ID_OBJECTION"/>
            </key>                  
    <one-to-many class="org.egov.license.domain.entity.objection.Activity"/>
 </bag>     
 <bag name="notices" cascade="all,all-delete-orphan" table="EGL_NOTICE" inverse="true">
            <key> 
            <column name="ID_OBJECTION"/>
            </key>                  
    <one-to-many class="org.egov.license.domain.entity.objection.Notice"/>
 </bag>    
</class>
<query name="LISENSEOBJECTION_BY_ID">
		from org.egov.license.domain.entity.objection.LicenseObjection where id=?
</query>
</hibernate-mapping>