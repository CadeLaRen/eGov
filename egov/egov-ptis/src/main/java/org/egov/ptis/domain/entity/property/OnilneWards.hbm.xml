<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.egov.ptis.domain.entity.property.OnlineWards" table="EGPT_ONLINE_DATE"  >
	<id
			name="id"
			column="id"
			type="int">
			<generator class="sequence">
			<param name="sequence">SEQ_ONLINE_DATE</param>
			</generator>
	</id>
	
	<property name="wardNum" column="WARD_NUM"/> 
	<property name="onlineDate" column="ONLINE_DATE"/>    
	<property name="wardId" column="WARD_ID"/>    
	
	</class>
</hibernate-mapping>