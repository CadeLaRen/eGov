<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.ptis.domain.entity.property.PropertyStatus" table="EGPT_STATUS">
		<id
			name="ID" column="ID_STATUS" type="int">
			<generator class="sequence">
					<param name="sequence">SEQ_STATUS</param>
			</generator>
		</id>
		<timestamp
			column="LASTUPDATEDTIMESTAMP"
			name="lastUpdatedTimeStamp"
			access="property"
			unsaved-value="null"
		/>
		<property
			name="name"
			column="STATUS_NAME"
			type="string"
		/>
		<property
			name="statusCode"
			column="CODE"
			type="string"
		/>
		
	</class>
</hibernate-mapping>