<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.egov.works.models.milestone.TrackMilestoneActivity" table="EGW_TRACK_MILESTONE_ACTIVITY" >
  		<id
			name="id"
			column="ID"
			type="long"
			unsaved-value="null">
			<generator class="sequence">
          	<param name="sequence">EGW_TRACK_MILESTONE_ACT_SEQ</param>
            </generator>
        </id>
        <many-to-one name="trackMilestone" class="org.egov.works.models.milestone.TrackMilestone" 
        	fetch="select" insert="false" update="false">
            <column name="TrackMilestone_ID" precision="22" scale="0" not-null="true"/>
        </many-to-one>
        <many-to-one name="milestoneActivity" class="org.egov.works.models.milestone.MilestoneActivity" fetch="select">
            <column name="Milestone_Activity_id" precision="22" scale="0" not-null="true"/>
        </many-to-one>
        <property name="status" type="java.lang.String">
            <column name="STATUS" length="50" />
        </property>
        <property name="complPercentage" type="java.math.BigDecimal" >
            <column name="COMPLETED_PERCENTAGE" precision="22" scale="0" />
        </property>
        <property name="remarks" type="java.lang.String">
            <column name="REMARKS" length="1024" />
        </property>
        <property name="completionDate" column="COMPLETETION_DATE " type="timestamp"/>
        <many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATEDBY"/>
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIEDBY"/>
        <property name="modifiedDate" column="MODIFIEDDATE" type="timestamp"/>
		<property name="createdDate" column="CREATEDDATE" />
    </class>
</hibernate-mapping>
