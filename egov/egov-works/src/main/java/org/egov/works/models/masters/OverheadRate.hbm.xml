<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.works.models.masters.OverheadRate"
		table="EGW_OVERHEAD_RATE">
		<id name="id" column="ID" type="long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">EGW_WORKS_OVERHEADRATE_SEQ</param>
			</generator>
		</id>
		<component name="lumpsumAmount">
			<property name="value" type="double" precision="2"
				scale="20" column="LUMPSUM_AMOUNT" />
		</component>
		<property name="percentage" column="PERCENTAGE" type="double" />
		<many-to-one name="overhead" column="OVERHEAD_ID" not-null="true" insert="false" update="false" /> 
		<component name="validity">
			<property name="startDate" type="date" />
			<property name="endDate" type="date" />
		</component>
	</class>
</hibernate-mapping>
