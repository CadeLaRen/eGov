<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.egov.works.models.estimate.EstimateTemplate" table="EGW_ESTIMATE_TEMPLATE">
		<id
			name="id"
			column="ID"
			type="long"
			unsaved-value="null">
			<generator class="native">
				<param name="sequence">EGW_ESTIMATE_TEMPLATE_SEQ</param>
			</generator>
		</id>
		<property name="code" type="string">
			<column name="CODE" length="256" not-null="true" unique="true"/> 
		</property>
		<property name="name" type="string">
			<column name="NAME" length="256" not-null="true" /> 
		</property>
		<property name="description" type="string">
			<column name="DESCRIPTION" length="1024" /> 
		</property>
		<property name="status" type="int">
			<column name="STATUS" /> 
		</property>
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" fetch="select"/>
		<property name="createdDate" column="CREATED_DATE" type="timestamp" />
		<property name="modifiedDate" column="MODIFIED_DATE" type="timestamp" />
       	<many-to-one name="workType"
			class="org.egov.commons.EgwTypeOfWork" fetch="select">
			<column name="WORKTYPE_ID" not-null="true" />
		</many-to-one>
        <many-to-one name="subType"
			class="org.egov.commons.EgwTypeOfWork" fetch="select"> 
			<column name="SUBTYPE_ID" />
		</many-to-one>
		<list name="activities" cascade="all,delete-orphan">
			<key column="ESTIMATE_TEMPLATE_ID" not-null="true" />
			<list-index column="activity_index" />
			<one-to-many
				class="org.egov.works.models.estimate.EstimateTemplateActivity" />
		</list>
       
   
</class>

	<query name="EstimateTemplateCodeUniqueCheck">
		<![CDATA[
     	select et.id from EstimateTemplate et where upper(et.code)=? 
    	]]>
	</query>
</hibernate-mapping>
