<?xml  version="1.0"?>
<CompanyDetail  name="companyDetail"
		 keyColumnName="ID"
		 keyRequiresQuote="No">
	
	<columnDefinition 
		name="name" 
		dataType="anyChar"
		isRequired="yes"
		maxLength="50"
		/>
	<columnDefinition 
		name="address1" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="50"
		/>
	<columnDefinition 
		name="address2" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="50"
		/>
	<columnDefinition 
		name="city" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="50"
		/>
	<columnDefinition 
		name="pin" 
		dataType="anyChar"
		defaultValue="0"
		isRequired="yes"
		maxLength=""
		/>
	<columnDefinition 
		name="state" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="50"
		/>

	<columnDefinition 
		name="phone" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="15"
		/>
	
	<columnDefinition 
		name="contactPerson" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="50"
		/>
	<columnDefinition 
		name="mobile" 
		dataType="anyChar"
		defaultValue="0"
		isRequired="yes"
		maxLength="10"
		/>
	<columnDefinition 
		name="fax" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="15"
		/>
	<columnDefinition 
		name="email" 
		dataType="email"
		defaultValue="abc@exilant.com"
		isRequired="yes"
		maxLength="50"
		/>
	
	<columnDefinition 
		name="narration" 
		dataType="anyChar"
		defaultValue="C"
		isRequired="yes"
		maxLength="250"
		/>
	
		
</CompanyDetail>