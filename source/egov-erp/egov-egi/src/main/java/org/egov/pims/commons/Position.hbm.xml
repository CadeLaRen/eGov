<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.pims.commons.Position" table="EG_POSITION">

		<id name="id" column="ID" type="int">
			<generator class="sequence">
				<param name="sequence">SEQ_POS</param>
			</generator>
		</id>
		<property name="name">
			<column name="POSITION_NAME" />
		</property>
		
		<property name="isPostOutsourced">
			<column name="ISPOST_OUTSOURCED" />
		</property>
		
		<many-to-one name="deptDesigId"
			class="org.egov.pims.commons.DeptDesig" fetch="select" cascade="save-update">
			<column name="ID_DEPTDESIG" />
		</many-to-one>
		
		<property name="modifiedDate" column="MODIFIEDDATE" type="timestamp"/>
     		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATEDBY"/>
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIEDBY"/>
		<property name="createdDate">
			<column name="CREATEDDATE"/>
		</property>	
		
		<!-- <many-to-one name="drawingOfficer" class = "org.egov.pims.commons.DrawingOfficer" column="ID_DRAWING_OFFICER" unique="true"/> -->

	</class>
</hibernate-mapping>
