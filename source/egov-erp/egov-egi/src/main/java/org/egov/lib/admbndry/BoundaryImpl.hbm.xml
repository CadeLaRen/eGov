<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.lib.admbndry.BoundaryImpl" table="EG_BOUNDARY">
		<id name="id" column="ID_BNDRY" type="int">
			<generator class="native">
				<param name="sequence">SEQ_EG_BNDRY</param>
			</generator>
		</id>
		<timestamp column="updatedTime" name="updatedTime" access="property" unsaved-value="null" />
		<property name="name" column="NAME" type="string" />
		<property name="bndryNameLocal" column="BNDRY_NAME_LOCAL" type="string" />
		<property name="materializedPath" column="MATERIALIZED_PATH" type="string" />
		<property name="boundaryNum" column="BNDRY_NUM" />
		<property name="fromDate" column="FROMDATE" type="date" />
		<property name="toDate" column="TODATE" type="date" />
		<property name="isHistory" column="IS_HISTORY" />
		<property name="bndryId" column="BNDRYID" />
		<property name="lng">
			<column name="LNG" scale="10" precision="14" />
		</property>
		<property name="lat">
			<column name="LAT" scale="10" precision="14" />
		</property>
		<many-to-one name="parent" class="org.egov.lib.admbndry.BoundaryImpl" column="PARENT" />
		<many-to-one name="boundaryType" class="org.egov.lib.admbndry.BoundaryTypeImpl" column="ID_BNDRY_TYPE" />
		<set name="children" order-by="NAME" inverse="true" cascade="all-delete-orphan">
			<key column="PARENT" />
			<one-to-many class="org.egov.lib.admbndry.BoundaryImpl" />
		</set>
	</class>
</hibernate-mapping>
