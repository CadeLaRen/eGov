<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.lib.address.model.Address" table="EG_ADDRESS">
		<id name="addressID" column="ADDRESSID" type="int">
			<generator class="native">
				<param name="sequence">SEQ_ADDRESS</param>
			</generator>
		</id>
		<timestamp column="LASTUPDATEDTIMESTAMP" name="lastUpdatedTimeStamp" access="property" unsaved-value="null" />
		<property name="streetAddress1" column="STREETADDRESS1" />
		<property name="streetAddress2" column="STREETADDRESS2" />
		<property name="block" column="BLOCK" />
		<property name="locality" column="LOCALITY" />
		<property name="cityTownVillage" column="CITYTOWNVILLAGE" />
		<property name="district" column="DISTRICT" />
		<property name="state" column="STATE" />
		<property name="streetAddress1Local" column="STREETADDRESS1LOCAL" />
		<property name="streetAddress2Local" column="STREERADDRESS2LOCAL" />
		<property name="blockLocal" column="BLOCKLOCAL" />
		<property name="localityLocal" column="LOCALITYLOCAL" />
		<property name="cityTownVillageLocal" column="CITYTOWNVILLAGELOCAL" />
		<property name="districtLocal" column="DISTRICTLOCAL" />
		<property name="stateLocal" column="STATELOCAL" />
		<property name="pinCode" column="PINCODE" />
		<property name="taluk" column="TALUKNAME" />
		<property name="talukLocal" column="TALUKLOCAL" />
		<many-to-one name="addTypeMaster" class="org.egov.lib.address.model.AddressTypeMaster" cascade="save-update,persist" column="ID_ADDRESSTYPEMASTER" />
		<property name="houseNo" column="HOUSENO" />
	</class>
</hibernate-mapping>