<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.lib.admbndry.CityWebsiteImpl" table="EG_CITY_WEBSITE">
		<id column="ID" name="id" type="integer">
			<generator class="native">
				<param name="sequence">SEQ_EG_CITY</param>
			</generator>
		</id>
		<property column="URL" generated="never" lazy="false" name="cityBaseURL" type="string" />
		<many-to-one cascade="save-update" class="org.egov.lib.admbndry.BoundaryImpl" name="boundaryId">
			<column name="BNDRYID" />
		</many-to-one>
		<property column="CITYNAME" generated="never" lazy="false" name="cityName" type="string" />
		<property column="CITYNAMELOCAL" generated="never" lazy="false" name="cityNameLocal" type="string" />
		<property column="ISACTIVE" generated="never" lazy="false" name="isActive" type="integer" />
		<property column="LOGO" generated="never" lazy="false" name="logo" 	type="string" />
	</class>
	<query name="CITY_BY_URL">FROM CityWebsiteImpl WHERE URL=:url</query>
</hibernate-mapping>
