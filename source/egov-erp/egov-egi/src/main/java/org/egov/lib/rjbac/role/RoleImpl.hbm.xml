<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.lib.rjbac.role.RoleImpl" table="EG_ROLE">
		<id name="id" column="ID" type="integer" unsaved-value="null">
			<generator class="native">
				<param name="sequence">SEQ_EG_ROLES</param>
			</generator>
		</id>
		<timestamp column="createddate" name="createddate" access="property" unsaved-value="null" />
		<property name="roleName" column="name" type="string" />
		<property name="roleDesc" column="description" type="string" />
		<property name="createdby" column="createdby" type="integer" />
		<property name="roleNameLocal" column="localName" type="string" />
		<property name="roleDescLocal" column="localDescription" type="string" />
		<set name="actions" table="EG_ROLEACTION_MAP" inverse="true">
			<key column="ROLEID" />
			<many-to-many class="org.egov.lib.rrbac.model.Action">
				<column name="ACTIONID" />
			</many-to-many>
		</set>
	</class>
</hibernate-mapping>