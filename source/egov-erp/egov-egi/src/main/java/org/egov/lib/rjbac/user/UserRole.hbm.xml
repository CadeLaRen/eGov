<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.lib.rjbac.user.UserRole" table="EG_USERROLE">
		<id name="id" column="ID" type="integer">
			<generator class="native">
				<param name="sequence">SEQ_EG_USERROLE</param>
			</generator>
		</id>
		<many-to-one name="role" class="org.egov.infra.admin.master.entity.Role" cascade="save-update">
			<column name="ID_ROLE" />
		</many-to-one>
		<many-to-one name="user" class="org.egov.infra.admin.master.entity.User" cascade="save-update">
			<column name="ID_USER" />
		</many-to-one>
		<property name="fromDate" column="FROMDATE" type="date" />
		<property name="toDate" column="TODATE" type="date" />
		<property name="isHistory" column="IS_HISTORY" />
	</class>
</hibernate-mapping>		