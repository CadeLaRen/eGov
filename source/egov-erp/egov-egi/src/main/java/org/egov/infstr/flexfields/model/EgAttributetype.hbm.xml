<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.infstr.flexfields.model.EgAttributetype" table="EG_ATTRIBUTETYPE">
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="sequence">
				<param name="sequence">SEQ_EG_ATTRIBUTETYPE</param>
			</generator>
		</id>
		<many-to-one name="egApplDomain" class="org.egov.infstr.flexfields.model.EgApplDomain">
			<column name="APPL_DOMAINID" not-null="true" />
		</many-to-one>
		<property name="attName" type="java.lang.String">
			<column name="ATT_NAME" not-null="true" />
		</property>
		<property name="attDatatype" type="java.lang.String">
			<column name="ATT_DATATYPE" not-null="true" />
		</property>
		<property name="defaultValue" type="java.lang.String">
			<column name="DEFAULT_VALUE" />
		</property>
		<property name="isrequired" type="java.lang.Long">
			<column name="ISREQUIRED" not-null="true" />
		</property>

		<set name="egAttributevalueses" inverse="true">
			<key>
				<column name="ATT_TYPEID" not-null="true" />
			</key>
			<one-to-many class="org.egov.infstr.flexfields.model.EgAttributevalues" />
		</set>
	</class>
</hibernate-mapping>
