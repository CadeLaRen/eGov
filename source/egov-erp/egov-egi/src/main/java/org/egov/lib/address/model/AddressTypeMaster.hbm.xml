<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.lib.address.model.AddressTypeMaster" table="EG_ADDRESS_TYPE_MASTER">
		<id name="addressTypeID" column="ID_ADDRESS_TYPE" type="int">
			<generator class="native">
				<param name="sequence">SEQ_ADDRESS_TYPE_MASTER</param>
			</generator>
		</id>
		<timestamp column="UPDATEDTIMESTAMP" name="updatedTimestamp" access="property" unsaved-value="null" />
		<property name="addressTypeName" column="NAME_ADDRESS_TYPE" />
		<property name="addressTypeNameLocal" column="NAME_ADDRESS_TYPE_LOCAL" />
	</class>
</hibernate-mapping>