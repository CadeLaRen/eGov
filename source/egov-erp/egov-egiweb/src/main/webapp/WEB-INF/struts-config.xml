  <global-exceptions>
	<exception
	key="error.generic"
	type="org.egov.EGOVException"
	path="/error/error.jsp"/>
  </global-exceptions>
  
  <global-forwards>
  	<forward name="login" path="/login.jsp" />
  	<forward name="error" path="/error/error.jsp" /> 
  	<forward name="failure" path="/error/error.jsp" />
  	<forward name="success" path = "/administration/rjbac/acknowledgement.jsp"/> 	
  </global-forwards>
       
  <action-mappings>
  	
		<!-- Action mapping for MoveBoundary, Used to Move a Boundary to another Parent Boundary -->
		 <action path="/AdminBoundary/moveBoundary"
  				    type="org.springframework.web.struts.DelegatingActionProxy" 
  				    scope="request"
  				    input="/AdminBoundary/moveBoundary.jsp">  				
  				 <forward name = "success" path = "/AdminBoundary/moveBoundary.jsp"/> 	 
				 <forward name = "AJAX" path = "/AdminBoundary/plainAjaxResponse.jsp"/> 	   				
		</action>

  	   <!-- common action element rjbac is used for user,role and department related features -->
  	
		<action path="/rjbac" 
				type="org.egov.infstr.client.administration.rjbac.role.RjbacAction" 	   	   
				scope="request">
				<forward name = "success" path = "/administration/rjbac/role/crtRole.jsp"/> 	   
				<forward name = "todeleteUser" path = "/administration/rjbac/user/deleteUser.jsp"/>
				<forward name = "todeleteDept" path = "/administration/rjbac/department/deleteDepartment.jsp"/>
				<forward name = "todeleteRole" path = "/administration/rjbac/role/deleteRole.jsp"/>
		</action>
		
	  		<action path="/error/errorPage"   
				scope="request" 
				type="org.egov.infstr.utils.ErrorMailAction"
				parameter="submitType"
				name="errorForm">  
				<forward name="success" path="/error/error.jsp"/> 
				<forward name="failure" path="/error.jsp"/>    
	  		</action> 
		
		
		<action path="/admin/chgPassword"
			type="org.springframework.web.struts.DelegatingActionProxy"  
			name="chgPasswordForm" 
			scope="request"
			validate="true"
			input="/administration/rjbac/user/changePassword.jsp">
		  <forward name = "success" path = "/administration/rjbac/acknowledgement.jsp"/>
		  <forward name = "incorrect" path = "/administration/rjbac/acknowledgement.jsp"/>
		</action> 
		
		<action path="/admin/directChgPassword" scope="request" parameter="/administration/rjbac/user/changePassword.jsp"
			type="org.apache.struts.actions.ForwardAction">
		</action> 


  	<!-- Role related action elements are defined  -->
	  	
	  	<action path="/Role" 
		   	    type="org.egov.infstr.client.administration.rjbac.role.UserRoleAction" 
		   	    name="roleForm" 
		   	    scope="session"
		   	    validate="false"	   	   
		   	   input="/administration/rjbac/role/createRole.jsp">
		       <forward name = "success" path = "/administration/rjbac/acknowledgement.jsp"/> 	   
		       <forward name = "toUpdateRole" path = "/administration/rjbac/role/createRole.jsp"/>
		       <forward name = "toViewRole" path = "/administration/rjbac/role/viewRole.jsp"/> 
		       <forward name = "toconfDelete" path = "/administration/rjbac/role/confirmDeleteRole.jsp"/>
	</action>
  		   <action path="/SetupRole" 
  				 type="org.egov.infstr.client.administration.rjbac.role.SetupRoleAction" 	   	   
  				 name="roleForm" 
  				 scope="session"
  				 input="/administration/rjbac/role/createRole.jsp">
  				 
  				<forward name = "viewRole" path = "/administration/rjbac/role/viewRoleCreation.jsp"/> 
  				<forward name = "updateRole" path = "/administration/rjbac/role/updateRole.jsp"/> 
  			       <forward name = "success" path = "/administration/rjbac/role/setupRole.jsp"/> 	   
  			      </action>
  	
		 
  		 <action path="/CreateRole"
  				    type="org.egov.infstr.client.administration.rjbac.role.RoleAction" 
  				    name="roleForm" 
  				    scope="session"
  				    validate="true"
  				    input="/administration/rjbac/role/createRole.jsp">
  				
  				 <forward name = "success" path = "/administration/rjbac/role/roleAcknowledgement.jsp"/> 	   
  				
		</action>
		
		
	<!-- Department related action elements are defined  -->
		
	<action path="/Department"   
