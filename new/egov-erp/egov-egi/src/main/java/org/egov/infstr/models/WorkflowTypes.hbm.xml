<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">
	<class name="org.egov.infstr.models.WorkflowTypes" table="EG_WF_TYPES">
		<id name="id" column="ID_TYPE" type="long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">EG_WF_TYPES_SEQ</param>
			</generator>
		</id>
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" not-null="true" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" not-null="true" fetch="select" />
		<many-to-one name="module" class="org.egov.infstr.commons.Module" column="MODULE_ID"></many-to-one>
		<property name="type" column="WF_TYPE" type="string" />
		<property name="link" column="WF_LINK" type="string" />
		<property name="displayName" column="DISPLAY_NAME" type="string" />
		<property name="fullyQualifiedName" column="FULL_QUALIFIED_NAME" type="string" />
		<property name="renderYN" column="RENDER_YN" type="char" />
		<property name="groupYN" column="GROUP_YN" type="char" />
		<property name="createdDate" column="CREATED_DATE" type="timestamp" />
		<property name="modifiedDate" column="MODIFIED_DATE" type="timestamp" />
	</class>
	<query name="MODULE_FOR_TYPE">
	<![CDATA[
     	select module.moduleName from org.egov.infstr.models.WorkflowTypes  where type=?   
    ]]>
	</query>
	<query name="TYPE_FOR_NAME">
	<![CDATA[
     	from org.egov.infstr.models.WorkflowTypes  where displayName = ?   
    ]]>
	</query>
	<query name="TYPE_LIKE_NAME">
	<![CDATA[
     	from org.egov.infstr.models.WorkflowTypes  where lower(displayName) like ?   
    ]]>
	</query>
</hibernate-mapping>
