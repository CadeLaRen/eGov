<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.egov.pgr.domain.entities.Escalation" table="EGGR_ESCALATION" >
  		<id name="id" type="java.lang.Long" unsaved-value="null">
            <column name="ID" not-null="true" />
           <generator class="native">
            	<param name="sequence">SEQ_EGGR_ESCALATION</param>
            </generator>
        </id>
        <many-to-one name="complaintType" class="org.egov.pgr.domain.entities.ComplaintTypes" fetch="select" >
            <column name="COMPLAINT_TYPE_ID" />
        </many-to-one>
       <many-to-one name="role" class="org.egov.lib.rjbac.role.RoleImpl" fetch="select" >
	   	 <column name="ROLE_ID" />
	   </many-to-one> 
         <property name="noOfDays" type="java.lang.Integer">
            <column name="NO_OF_DAYS" />
        </property>    
    </class>  
</hibernate-mapping>
