<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.egov.pgr.domain.entities.ForwardTracker" table="EGGR_FORWARD_TRACKER" >
        <id name="id" type="java.lang.Long">
            <column name="FID" />
            <generator class="sequence">
            	<param name="sequence">SEQ_EGGR_COMPLAINTGROUP</param>
            </generator>
        </id>
        <many-to-one name="fromOfficer" class="org.egov.lib.rjbac.user.UserImpl" fetch="select">
            <column name="FROMOFFICERID" not-null="true" />
        </many-to-one>
        <many-to-one name="toOfficer" class="org.egov.lib.rjbac.user.UserImpl" fetch="select">
            <column name="TOOFFICERID"/>
        </many-to-one>
        <many-to-one name="complaintDetails" class="org.egov.pgr.domain.entities.ComplaintDetails" fetch="select">
            <column name="complaintid" not-null="true" />
        </many-to-one>
        <property name="dateForwarded" type="java.util.Date">
            <column name="dateforwarded"  />
        </property>
        <property name="comments" type="java.lang.String">
            <column name="comments" length="256" />
        </property>
        <property name="notes" type="java.lang.String">
            <column name="notes" length="256" />
        </property>
        <many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" fetch="select">
            <column name="MODIFIEDBY" not-null="true" />
        </many-to-one>
       
    </class>
</hibernate-mapping>
