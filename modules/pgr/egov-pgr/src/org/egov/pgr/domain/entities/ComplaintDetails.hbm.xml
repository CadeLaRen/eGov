<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.egov.pgr.domain.entities.ComplaintDetails" table="EGGR_COMPLAINTDETAILS" >
        <id name="id" type="java.lang.Long" unsaved-value="null">
            <column name="COMPLAINTID" not-null="true" />
           <generator class="native">
            	<param name="sequence">SEQ_EGGR_COMPLAINTDETAIL</param>
            </generator>
        </id>
        <many-to-one name="priority" class="org.egov.pgr.domain.entities.Priority" fetch="select">
            <column name="PRIORITY" />
        </many-to-one>
        <many-to-one name="topLevelbndry" class="org.egov.lib.admbndry.BoundaryImpl" fetch="select">
            <column name="TOPLEVELBNDRY" />
        </many-to-one>
        <many-to-one name="boundary" class="org.egov.lib.admbndry.BoundaryImpl" fetch="select">
            <column name="BNDRY" />
        </many-to-one>
        <many-to-one name="compReceivingModes" class="org.egov.pgr.domain.entities.CompReceivingModes" fetch="select">
            <column name="ID_MODE"  not-null="true" />
        </many-to-one>
        <property name="complaintNumber" type="java.lang.String">
            <column name="COMPLAINTNUMBER" length="64" unique="true" />
        </property>
        <property name="complaintDate" type="java.util.Date">
            <column name="COMPLAINTDATE"  />
        </property>
        <property name="firstName" type="java.lang.String">
            <column name="COMPLAINANTFIRSTNAME" length="32" />
        </property>
        <property name="initials" type="java.lang.String">
            <column name="COMPLAINANINITIALS" length="32" />
        </property>
        <property name="lastName" type="java.lang.String">
            <column name="COMPLAINANTLASTNAME" length="32" />
        </property>
        <property name="address" type="java.lang.String">
            <column name="COMPLAINANTADDRESS" length="256" />
        </property>
        <property name="city" type="java.lang.String">
            <column name="COMPLAINANTCITY" length="64" />
        </property>
        <property name="telePhone" type="java.lang.String">
            <column name="COMPLAINANTTELEPHONE" length="32" />
        </property>
        <property name="email" type="java.lang.String">
            <column name="COMPLAINANTEMAIL" length="64" />
        </property>
        <many-to-one name="complaintType" class="org.egov.pgr.domain.entities.ComplaintTypes" fetch="select">
            <column name="COMPLAINTTYPE" />
        </many-to-one>
       
        <property name="details" type="java.lang.String">
            <column name="COMPLAINTDETAILS" length="1000" />
        </property>
        <property name="title" type="java.lang.String">
            <column name="COMPLAINTTITLE" length="1000" />
        </property>
        <property name="refComplaintId" type="java.lang.Integer">
            <column name="REFCOMPLAINTID" />
        </property>
         <many-to-one name="receivingCenter" class="org.egov.pgr.domain.entities.ComplaintReceivingCenter" fetch="select">
            <column name="RECEIVINGCENTERID" />
        </many-to-one>
       
        <property name="complaintlocation" type="java.lang.String">
            <column name="COMPLAINTLOCATION" length="256" />
        </property>
         <many-to-one name="department" class="org.egov.lib.rjbac.dept.DepartmentImpl" fetch="select">
            <column name="DEPTID" />
        </many-to-one>
       <!--  <property name="landmark" type="java.lang.String">
            <column name="COMPLAINTLANDMARK" length="256" />
        </property>
        <property name="severity" type="java.lang.Integer">
            <column name="severity"  />
        </property>
        <property name="firstNameLocal" type="java.lang.String">
            <column name="complainantfirstname_local" length="32" />
        </property>
        <property name="initialsLocal" type="java.lang.String">
            <column name="complainaninitials_local" length="32" />
        </property>
        <property name="lastnameLocal" type="java.lang.String">
            <column name="complainantlastname_local" length="32" />
        </property>
        <property name="addressLocal" type="java.lang.String">
            <column name="complainantaddress_local" length="256" />
        </property>
        <property name="cityLocal" type="java.lang.String">
            <column name="complainantcity_local" length="64" />
        </property>
        <property name="detailsLocal" type="java.lang.String">
            <column name="complaintdetails_local" length="1000" />
        </property>
        <property name="locationLocal" type="java.lang.String">
            <column name="complaintlocation_local" length="256" />
        </property>
        <property name="landmarkLocal" type="java.lang.String">
            <column name="complaintlandmark_local" length="256" />
        </property>
        <property name="titleLocal" type="java.lang.String">
            <column name="complainttitle_local" length="32" />
        </property> -->
        <property name="mobileNumber" type="java.lang.String">
            <column name="mobilenumber" length="32" />
        </property>
        <property name="mode" type="java.lang.String">
            <column name="complaintmode" length="32" />
        </property>
        <property name="pincode" type="java.lang.String">
            <column name="pincode" length="32" />
        </property>
        <property name="othervalue" type="java.lang.String">
            <column name="othervalue" length="256" />
        </property>
       <!--  <property name="extrafield1" type="java.lang.String">
            <column name="extrafield1" length="1024" />
        </property> -->
        <property name="isEscalated" type="java.lang.Integer">
            <column name="isescalated"  not-null="true" />
        </property>
        <property name="escalatedTime" type="java.util.Date">
            <column name="escalatedtime" />
        </property>
        <property name="expiryDate" type="java.util.Date">
            <column name="EXPIRYDATE" />
        </property>
       
         <many-to-one name="locBndry" class="org.egov.lib.admbndry.BoundaryImpl" fetch="select">
            <column name="LOCBNDRYID" />
        </many-to-one>
        
		<one-to-one name="redressal" class="org.egov.pgr.domain.entities.RedressalDetails" property-ref="complaintDetails" cascade="all" /> 
        
        <property name="imageDocNumber" type="java.lang.String">
            <column name="image_document_number" />
        </property>
       
        
       <component name="geoLocationDetails" class="org.egov.infstr.models.GeoLocation">
			<component name="geoLatLong" class="org.egov.infstr.models.GeoLatLong">
				<property name="latitude" column="LATITUDE" type="java.math.BigDecimal" precision="18" ></property>
				<property name="longitude" column="LONGITUDE" type="java.math.BigDecimal" precision="18" ></property>
			</component>
			<property name="info1" column="GEOLOCINFO1" type="java.lang.String" length="1024"></property>
			<property name="info2" column="GEOLOCINFO2" type="java.lang.String" length="1024"></property>
			<property name="info3" column="GEOLOCINFO3" type="java.lang.String" length="1024"></property>
			<property name="info4" column="GEOLOCINFO4" type="java.lang.String" length="1024"></property>
			<property name="urlRedirect" column="GEOLOCINFOURL1" type="java.lang.String" length="1024"></property>
			<property name="urlDisplay" column="GEOLOCINFOURL2" type="java.lang.String" length="1024"></property>
		</component>
         <many-to-one name="state" class="org.egov.infstr.models.State" cascade="save-update,persist" fetch="select">
            <column name="ID_STATE" />
        </many-to-one>
         <many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY"/>
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY"/>
        <property name="createdDate" type="java.util.Date">
            <column name="CREATED_DATE"  />
        </property>
        <property name="modifiedDate" type="java.util.Date">
            <column name="MODIFIED_DATE"  />
        </property>
    </class>
</hibernate-mapping>
