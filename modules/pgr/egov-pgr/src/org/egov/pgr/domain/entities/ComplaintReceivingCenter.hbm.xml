<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.egov.pgr.domain.entities.ComplaintReceivingCenter" table="EGGR_COMPLAINTRECEIVINGCENTER" >
        <id name="id" type="java.lang.Long">
            <column name="centerid" />
            <generator class="sequence">
            	<param name="sequence">SEQ_EGGR_COMPLAINTRECEIVINGCEN</param>
            </generator>
        </id>
        <property name="centerType" type="java.lang.Integer">
            <column name="CENTERTYPE"  />
        </property>
        <property name="centerName" type="java.lang.String">
            <column name="CENTERNAME" length="64" />
        </property>
        <property name="centerAddress" type="java.lang.String">
            <column name="CENTERADDRESS" length="128" />
        </property>
        <property name="centerManagerId" type="java.lang.Integer">
            <column name="CENTERMANAGERID"  />
        </property>
        <property name="centerNameLocal" type="java.lang.String">
            <column name="CENTERNAME_LOCAL" length="64" />
        </property>
        <property name="centerAddressLocal" type="java.lang.String">
            <column name="CENTERADDRESS_LOCAL" length="128" />
        </property>
        <property name="bndryId" type="java.lang.Integer">
            <column name="BNDRYID" />
        </property>
        <property name="isActive" type="java.lang.String">
            <column name="IS_ACTIVE" length="1" />
        </property>
    </class>
     <query name="getAllCompReceivingCenter">
		<![CDATA[
         from ComplaintReceivingCenter where isActive=1
    ]]>
	</query>
</hibernate-mapping>
