<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.egov.pgr.domain.entities.CompReceivingModes" table="EGGR_COMP_RECEIVING_MODES" >
        <id name="id" type="java.lang.Long">
            <column name="comp_id" />
             <generator class="sequence">
            	<param name="sequence">SEQ_EGGR_COMPLAINTRECEIVINGMODES</param>
            </generator>
        </id>
        <property name="compMode" type="java.lang.String">
            <column name="COMP_MODE" length="64" not-null="true" />
        </property>
      
    </class>
     <query name="getAllCompReceivingModes">
		<![CDATA[
         from CompReceivingModes order by compMode
    ]]>
	</query>
</hibernate-mapping>
