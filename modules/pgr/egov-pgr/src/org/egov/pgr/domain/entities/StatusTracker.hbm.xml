<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.egov.pgr.domain.entities.StatusTracker" table="EGGR_STATUS_TRACKER" >
        <id name="id" type="java.lang.Long">
            <column name="PKID"  />
             <generator class="sequence">
            	<param name="sequence">SEQ_EGGR_STATUSTRACKER</param>
            </generator>
        </id>
        <many-to-one name="fromUserId" class="org.egov.lib.rjbac.user.UserImpl"  fetch="select">
            <column name="FROMUSERID"  not-null="true" />
        </many-to-one>
        <many-to-one name="redressalDetails" class="org.egov.pgr.domain.entities.RedressalDetails" fetch="select">
            <column name="REDRESSALID"  not-null="true" />
        </many-to-one>
        <many-to-one name="toUserId" class="org.egov.lib.rjbac.user.UserImpl"  fetch="select">
            <column name="TOUSERID"  not-null="true" />
        </many-to-one>
        <many-to-one name="complaintStatus" class="org.egov.pgr.domain.entities.ComplaintStatus" fetch="select">
            <column name="STATUSID" not-null="true" />
        </many-to-one>
        <many-to-one name="communication" class="org.egov.pgr.domain.entities.Communication" fetch="select">
            <column name="COMMUNICATIONID"  />
        </many-to-one>
        <property name="orderId" type="java.lang.Integer">
            <column name="ORDERID" not-null="true" />
        </property>
        <property name="timestamp" type="java.util.Date">
            <column name="TIMESTAMP" />
        </property>
        <property name="modifiedBy" type="java.math.BigDecimal">
            <column name="MODIFIEDBY" />
        </property>
    </class>
</hibernate-mapping>
