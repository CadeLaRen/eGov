<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.egov.pgr.domain.entities.ComplaintGroup" table="EGGR_COMPLAINTGROUP">
        <id name="id" type="java.lang.Long">
            <column name="ID_COMPLAINTGROUP"  />
           <generator class="sequence">
            	<param name="sequence">SEQ_EGGR_COMPLAINTGROUP</param>
            </generator>
        </id>
        <property name="complaintGroupName" type="java.lang.String">
            <column name="COMPLAINTGROUPNAME" length="64" not-null="true" />
        </property>
        <property name="isActive" type="java.lang.String">
            <column name="IS_ACTIVE" length="1" />
        </property>
    </class>
</hibernate-mapping>
