<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.egov.pgr.domain.entities.RedressalDetails" table="EGGR_REDRESSALDETAILS">
        <id name="id" type="java.lang.Long">
            <column name="redressalid" />
         <generator class="sequence">
            	<param name="sequence">SEQ_EGGR_REDRESSALDETAILS</param>
            </generator>
        </id>
        <many-to-one name="complaintDetails" class="org.egov.pgr.domain.entities.ComplaintDetails" fetch="select">
            <column name="COMPLAINTID" not-null="true" />
        </many-to-one>
        <many-to-one name="redressalOfficer" class="org.egov.lib.rjbac.user.UserImpl" fetch="select">
            <column name="REDRESSALOFFICERID"  not-null="true" />
        </many-to-one>
        <many-to-one name="complaintStatus" class="org.egov.pgr.domain.entities.ComplaintStatus" fetch="select">
            <column name="COMPLAINTSTATUSID" />
        </many-to-one>
        <property name="responsedate" type="java.util.Date">
            <column name="RESPONSEDATE"  />
        </property>
        <property name="response" type="java.lang.String">
            <column name="response" length="1000" />
        </property>
        <property name="extrafield1" type="java.lang.String">
            <column name="extrafield1" length="32" />
        </property>
        <property name="extrafield2" type="java.lang.String">
            <column name="extrafield2" length="32" />
        </property>
        <property name="extrafield3" type="java.lang.String">
            <column name="extrafield3" length="32" />
        </property>
        <property name="actiontype" type="java.lang.Integer">
            <column name="actiontype"  />
        </property>
        <property name="responseLocal" type="java.lang.String">
            <column name="RESPONSE_LOCAL" length="10" />
        </property>
       <many-to-one class="org.egov.pims.commons.Position" fetch="select" name="position">
   			<column name="POSTITIONID" precision="32" scale="0"/>
 	   </many-to-one>
    </class>
</hibernate-mapping>
