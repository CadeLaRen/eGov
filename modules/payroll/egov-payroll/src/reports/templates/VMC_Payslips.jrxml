	<queryString>
		<![CDATA[select pay.ID,pay.GROSS_PAY,pay.NET_PAY,ph.NAME as payscalename,pay.WORKINGDAYS,pay.NUMDAYS,pay.MONTH,
emp.NAME,emp.EMP_FIRSTNAME,emp.EMP_LASTNAME,emp.EMP_MIDDLENAME,emp.CODE,emp.gender,
emp.GPF_AC_NUMBER,emp.pan_number as panNumber,to_char(emp.date_of_birth ,'DD-MM-YYYY')as DOB ,
to_char(emp.date_of_first_appointment ,'DD-MM-YYYY')as DOFA,emp.GPF_AC_NUMBER as pfNumber,des.DESIGNATION_NAME ,funcry.code as Func_CODE,funcry.Name as FUNC_NAME ,bankDet.account_number
as acNumber,bnk.name as bankName,br.branchname as branchName ,
dept.DEPT_NAME as dep,gm.ORDER_NO
from EGPAY_EMPPAYROLL pay,
EGPAY_PAYSCALE_EMPLOYEE payscale,
EGPAY_PAYSCALE_HEADER ph,egeis_billnumber_master ebm,
eg_employee emp left join egeis_bank_det bankDet on bankDet.id=emp.id
LEFT JOIN BANK bnk ON bnk.ID=bankDet.bank LEFT JOIN BANKBRANCH br ON br.ID=bankDet.branch AND br.bankid=bnk.ID,
eg_department dept,  eg_designation des,
functionary funcry right join eg_emp_assignment assn on assn.id_functionary=funcry.id, EGEIS_GRADE_MSTR gm where  des.DESIGNATIONID = assn.DESIGNATIONID
and pay.ID_EMPLOYEE = emp.ID and pay.MONTH = $P{MONTH}
AND pay.status !=$P{STATUS}
AND pay.financialyearid = $P{FINYR}  AND assn.ID = pay.id_emp_assignment
AND pay.id_employee = payscale.ID_EMPLOYEE AND payscale.ID = (SELECT ID FROM EGPAY_PAYSCALE_EMPLOYEE str
WHERE str.EFFECTIVEFROM IN (SELECT MAX(stemp.effectiveFrom) FROM EGPAY_PAYSCALE_EMPLOYEE stemp WHERE
stemp.effectiveFrom<= pay.TODATE AND stemp.ID_EMPLOYEE=emp.ID) AND str.id_employee =emp.ID )AND
payscale.ID_PAYHEADER=ph.ID and dept.ID_DEPT = assn.MAIN_DEPT AND des.DESIGNATIONID = assn.DESIGNATIONID
and ebm.id=pay.id_billnumber and ebm.billnumber=decode($P{billId},'',ebm.billnumber,$P{billId})
AND ( $P{DEPT_ID} IS NULL OR  $P{DEPT_ID} = assn.MAIN_DEPT ) and gm.GRADE_ID=assn.GRADE_ID
order by gm.ORDER_NO,des.DESIGNATION_NAME]]>
