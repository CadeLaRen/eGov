	<queryString>
		<![CDATA[SELECT pay.ID,pay.GROSS_PAY,pay.NET_PAY,pay.MONTH,emp.EMP_FIRSTNAME,emp.EMP_LASTNAME,emp.EMP_MIDDLENAME,emp.CODE,des.DESIGNATION_NAME,emp.NAME,funcry.code as Func_CODE,funcry.Name as FUNC_NAME,bank.account_number as bank,dept.DEPT_NAME as dep,emp.payment_type
  FROM egpay_emppayroll pay, eg_employee emp left join egeis_bank_det bank on bank.id=emp.id,
  eg_department dept,
  eg_designation des,
  functionary funcry right join eg_emp_assignment assn on   assn.id_functionary=funcry.id
 WHERE pay.MONTH = $P{MONTH}
   AND pay.status != $P{STATUS}
   AND pay.financialyearid = $P{FINYR}
   AND id_employee = emp.ID
   AND assn.ID = pay.id_emp_assignment
   and dept.ID_DEPT = assn.MAIN_DEPT
   and des.DESIGNATIONID = assn.DESIGNATIONID  and ( $P{DEPT_ID} IS NULL or $P{DEPT_ID} = assn.MAIN_DEPT )]]>
	</queryString>
