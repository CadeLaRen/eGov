	<queryString>
		<![CDATA[select pay.ID,pay.GROSS_PAY,pay.NET_PAY,pay.MONTH,emp.NAME,emp.EMP_FIRSTNAME,emp.EMP_LASTNAME,emp.EMP_MIDDLENAME,emp.payment_type ,
   emp.CODE,des.DESIGNATION_NAME ,funcry.code as Func_CODE,funcry.Name as FUNC_NAME ,bank.account_number as bank,dept.DEPT_NAME as dep
 from EGPAY_EMPPAYROLL pay,eg_employee emp left join egeis_bank_det bank on bank.id=emp.id , eg_department dept,
  eg_designation des,
  functionary funcry right join eg_emp_assignment assn on   assn.id_functionary=funcry.id
 where des.DESIGNATIONID = assn.DESIGNATIONID
 and pay.ID_EMPLOYEE = emp.ID and pay.MONTH = $P{MONTH}
   AND pay.status != $P{STATUS}
   AND pay.financialyearid = $P{FINYR}
   AND assn.ID = pay.id_emp_assignment
   and id_employee = $P{EMPID}
   and dept.ID_DEPT = assn.MAIN_DEPT]]>
	</queryString>
