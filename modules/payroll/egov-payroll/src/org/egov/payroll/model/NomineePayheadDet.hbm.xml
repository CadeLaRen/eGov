<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.egov.payroll.model.NomineePayheadDet" table="EGPAY_NOMINEE_PAYHEAD_DET">
        <id name="id" type="long">
           <column name="ID" precision="32" scale="0" />
           <generator class="native">
	   	  	<param name="sequence">SEQ_EGPAY_NOMINEE_PAYHEAD_DET</param>
           </generator>
        </id>
              
	 	<many-to-one name="empNomineeMaster" class="org.egov.pims.model.EmployeeNomineeMaster" fetch="select">
	    	<column name="ID_NOMINEE_MASTER" precision="32" scale="0" />
     	</many-to-one>
       
      	<many-to-one name="NominationHeader" class="org.egov.payroll.model.NominationHeader" fetch="select">
            <column name="ID_NOMINATION_HEADER" precision="32" scale="0" />
      	</many-to-one>
       
        <property name="pct" type="big_decimal">
            <column name="PCT"/>
        </property>
       
	  
	 	<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" fetch="select">
  	 		<column name="CREATED_BY" precision="12" scale="0"  />
	 	</many-to-one>
     
     	<property name="createdDate" type="date">
          <column name="CREATED_DATE" length="7"  />
     	</property>
     
     	<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" fetch="select">
  	 		<column name="MODIFIED_BY" precision="12" scale="0"  />
	 	</many-to-one>  
	 	
	 	<property name="modifiedDate" type="java.util.Date">
            <column name="MODIFIED_DATE" not-null="true" />
    	</property>
    	
    </class>
</hibernate-mapping>
