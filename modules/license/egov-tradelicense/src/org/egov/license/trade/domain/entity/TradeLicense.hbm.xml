<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <subclass name="org.egov.license.trade.domain.entity.TradeLicense" extends="org.egov.license.domain.entity.License"  discriminator-value="tradelicense">
        <bag name="installedMotorList" inverse="true" table="EGL_MOTOR_DETAILS" cascade="all,delete-orphan">
			<key>
				<column name="ID_LICENSE"></column>
			</key>
			<one-to-many class="org.egov.license.domain.entity.MotorDetails" />
		</bag>
		<join table="EGL_TRADE_LICENSE">
			<key column="ID" />
			<property name="motorInstalled" column="ISMOTORINSTALLED" type="boolean" />
			<property name="totalHP" column="TOTAL_HP" type="java.math.BigDecimal" />
			<property name="hotelGrade" column="HOTEL_GRADE" type="java.lang.String" />
			<property name="sandBuckets" column="SAND_BUCKETS"  />
			<property name="waterBuckets" column="WATER_BUCKETS"  />
			<property name="dcpExtinguisher" column="DCP_EXTINGUISHER"  />
			<property name="nocNumber" column="NOC_NUMBER"/>
			<property name="isCertificateGenerated" column="ISCERTIFICATEGENERATED"/>
			<many-to-one name="state" class="org.egov.infstr.models.State" cascade="all">
				<column name="STATE_ID" />
			</many-to-one>
		</join>
    </subclass>
</hibernate-mapping>
