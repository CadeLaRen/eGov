<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.egov.license.domain.entity">
	<joined-subclass name="LicenseDemand" table="EGL_LICENSE_DEMAND" extends="org.egov.demand.model.EgDemand">
		<key column="ID_DEMAND"  />
		<many-to-one name="license" column="ID_LICENSE" not-null="true" class="org.egov.license.domain.entity.License"/>
		<many-to-one name="egInstallmentMaster" class="org.egov.commons.Installment" >
            <column name="ID_INSTALLMENT"  />
        </many-to-one> 
		<property name="renewalDate" column="RENEWAL_DATE" type="java.util.Date"/>
		<property name="isLateRenewal" column="IS_LATERENEWAL"  type="character"/>
	</joined-subclass>
</hibernate-mapping>