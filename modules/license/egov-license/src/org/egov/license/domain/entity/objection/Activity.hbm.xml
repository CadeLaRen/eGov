<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">
	<class name="org.egov.license.domain.entity.objection.Activity" table="EGL_ACTIVITY" >
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native">
				<param name="sequence">SEQ_EGL_ACTIVITY</param>
			</generator>
		</id>
		<property name="details" type="string" />
		<property name="activityDate" type="date" />
		<property name="expectedDateOfResponse" type="date" />
		<property name="remarks" type="string" />
		<property name="docNumber" type="string" />
		<many-to-one name="objection" column="ID_OBJECTION" class="org.egov.license.domain.entity.objection.LicenseObjection" cascade="all" />
		<property name="type" type="string" />
		<property name="createdDate" column="CREATEDDATE" type="timestamp" />
		<property name="modifiedDate" column="LASTMODIFIEDDATE" type="timestamp" />
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATEDBY" not-null="true" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="LASTMODIFIEDBY" not-null="true" fetch="select" />
	</class>
</hibernate-mapping>