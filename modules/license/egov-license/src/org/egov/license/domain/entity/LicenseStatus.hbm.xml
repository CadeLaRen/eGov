<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.license.domain.entity.LicenseStatus" table="EGL_MSTR_STATUS">
		<id name="ID" column="ID_STATUS" type="int">
			<generator class="sequence">
				<param name="sequence">SEQ_EGL_MSTR_STATUS</param>
			</generator>
		</id>
		<timestamp column="LASTUPDATEDTIMESTAMP" name="lastUpdatedTimeStamp" access="property" unsaved-value="null" />
		<property name="name" column="STATUS_NAME" type="string" />
		<property name="statusCode" column="CODE" type="string" />
		<property name="active" column="IS_ACTIVE" type="yes_no" />
		<property name="orderId">
			<column name="ORDER_ID" />
		</property>
	</class>
</hibernate-mapping>