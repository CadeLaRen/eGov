<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.license.domain.entity.MajorWorks" table="EGL_MAJOR_WORKS">
		<id name="id" type="java.lang.Long">
			<column name="ID" precision="22" scale="0" />
			<generator class="native">
				<param name="sequence">SEQ_EGL_MAJOR_WORKS</param>
			</generator>
		</id>
		<many-to-one name="license" class="org.egov.license.domain.entity.License" fetch="select">
			<column name="ID_LICENSE" precision="22" scale="0" not-null="true" />
		</many-to-one>
		<property name="fireFighterSystem">
			<column name="FIRE_FIGHTER_SYSTEM" />
		</property>
		<property name="detection">
			<column name="DETECTION" />
		</property>
		<property name="passiveProtection">
			<column name="PASSIVE_PROTECTION" />
		</property>
	</class>
</hibernate-mapping>
