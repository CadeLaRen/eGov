<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.license.domain.entity.LicenseCategory" table="EGL_MSTR_CATEGORY">
		<id column="ID" name="id" type="long">
			<generator class="native">
				<param name="sequence">SEQ_EGL_MSTR_CATEGORY</param>
			</generator>
		</id>
		<property generated="never" lazy="false" name="createdDate" type="java.util.Date">
			<column length="7" name="CREATEDDATE" not-null="true" />
		</property>
		<property generated="never" lazy="false" name="modifiedDate" type="java.util.Date">
			<column name="LASTMODIFIEDDATE" not-null="true" />
		</property>
		<many-to-one class="org.egov.lib.rjbac.user.UserImpl" column="CREATEDBY" fetch="select" name="createdBy"
			not-null="true" />
		<many-to-one class="org.egov.lib.rjbac.user.UserImpl" column="LASTMODIFIEDBY" fetch="select" name="modifiedBy"
			not-null="true" />
		<property generated="never" lazy="false" name="name" type="java.lang.String">
			<column length="256" name="NAME" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
