<?xml  version="1.0"?>
<AssetType  name="assetType"
		 keyColumnName="ID"
		 keyRequiresQuote="No">
	
	<columnDefinition 
		name="code" 
		dataType="alphaNumeric"
		isRequired="yes"
		maxLength="10"
		/>
	<columnDefinition 
		name="name" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="50"
		/>
	<columnDefinition 
		name="description" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="50"
		/>	
</AssetType>