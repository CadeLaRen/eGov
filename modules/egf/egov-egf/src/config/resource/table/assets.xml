		name="dateOfCapitalization" 
		dataType="anyDate"
		maxLength="20"
		/>
	<columnDefinition 
		name="depreciationRate" 
		dataType="unsignedDecimal"
		defaultValue=""
		isRequired="yes"
		maxLength="10"
		/>
	<columnDefinition 
		name="scheme" 
		dataType="unsignedInt"
		defaultValue=""
		isRequired="yes"
		maxLength="10"
		/>
