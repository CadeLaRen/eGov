<?xml  version="1.0"?>
<Assets  name="assets"
		 keyColumnName="ID"
		 keyRequiresQuote="No" >

	<columnDefinition 
		name="assetType" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="20"
		/>
	<columnDefinition 
		name="assetCategory" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="20"
		/>
	<columnDefinition 
		name="code" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="10"
		/>
	<columnDefinition 
		name="name" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="250"
		/>
	<columnDefinition 
		name="description" 
		dataType="anyChar"
		maxLength="250"
		/>
	<columnDefinition 
		name="bookValue" 
		dataType="unsignedDecimal"
		defaultValue=""
		isRequired="yes"
		maxLength="20"
