<?xml  version="1.0"?>
 <billCollector name="billCollector"
		 keyColumnName="ID"
		 keyRequiresQuote="No">
	  <columnDefinition 
		name="code" 
		dataType="alphaNumeric"
		 isRequired="yes"
		 maxLength="10"
		 /> 
	  <columnDefinition
		 name="name" 
		 dataType="anyChar"
		 isRequired="yes"
		 maxLength="10"
		 /> 
	  <columnDefinition
		 name="departmentId"
		 dataType="unsignedInt"
		 defaultValue=""
		 isRequired="yes"
		 maxLength="4"
		 /> 
	  <columnDefinition
		 name="cashInHand"
		 dataType="unsignedInt"
		 defaultValue=""
		 isRequired="yes"
		 maxLength="4"
		 /> 
	  <columnDefinition
		 name="chequeInHand"
		 dataType="unsignedInt"
		 defaultValue=""
		 isRequired="yes"
		 maxLength="4"
		 /> 
	  <columnDefinition
		 name="type"
		 dataType="anyChar"
		 defaultValue="0"
		 isRequired="yes"
		 maxLength="15"
		 /> 
	  <columnDefinition
		 name="zoneId"
		 dataType="anyChar"
		 defaultValue=""	
		 isRequired="yes"
		 maxLength="4" 
		/> 
	  <columnDefinition
		 name="regionId"
		 dataType="anyChar"
		 defaultValue=""
		 isRequired="yes"
		 maxLength="4" 
		/> 
	  <columnDefinition
		 name="narration"
		 dataType="anyChar"
		 defaultValue=""
		 isRequired="yes"
		 maxLength="250" 
		/> 
	<columnDefinition
		 name="isActive"
		 dataType="boolean"
		 defaultValue="1"
		 isRequired="yes" 
		/> 
	  
  </billCollector>