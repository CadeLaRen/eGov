<?xml  version="1.0"?>
<egf_tax_code name="egf_tax_code"
		 keyColumnName="ID"
		 keyRequiresQuote="No">
	<columnDefinition 
		name="code" 
		dataType="alphaNumeric"
		isRequired="yes"
		maxLength="10"
		/>
	<columnDefinition 
		name="name" 
		dataType="anyChar"
		isRequired="yes"
		maxLength="50"
		/>
	<columnDefinition 
		name="accrued" 
		dataType="unSignedInt"
		isRequired="No"
		maxLength="1"
		/>
</egf_tax_code>