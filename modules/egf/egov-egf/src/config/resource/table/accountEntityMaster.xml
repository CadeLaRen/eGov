<?xml  version="1.0"?>
<AccountEntityMaster name="accountEntityMaster"
		 keyColumnName="ID"
		 keyRequiresQuote="No">
	<columnDefinition 
		name="name" 
		dataType="anyChar"
		isRequired="yes"
		maxLength="50"
		/>
	<columnDefinition 
		name="code" 
		dataType="anyChar"
		maxLength="50"
		/>
	<columnDefinition 
		name="narration" 
		dataType="anyChar"
		defaultValue=""
		isRequired="yes"
		maxLength="50"
		/>	
				
	<columnDefinition 
		name="detailTypeId" 
		dataType="unsignedInt"	
		defaultValue="0"	
		isRequired="yes"
		maxLength="10"				
		/>
	
</AccountEntityMaster>