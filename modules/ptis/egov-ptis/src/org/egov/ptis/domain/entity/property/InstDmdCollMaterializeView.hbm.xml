		<property name="fireTax" column="FIRETAX"/>
		<property name="sewerageTax" column="SEWERAGETAX"/>
		<property name="lightTax" column="LIGHTTAX"/>
		<property name="bigBldgTax" column="BIGBLDGTAX"/>
		<property name="chqBouncePenalty" column="CHQBUNCPENALTY"/>
		<property name="penaltyFine" column="PENALTYFINE"/>
		<property name="generalTaxColl" column="GENERALTAXCOLL"/>
		<property name="egsTaxColl" column="EGSTAXCOLL"/>
		<property name="eduCessResdTaxColl" column="EDUCESSRESDTAXCOLL"/>
		<property name="eduCessNonResdTaxColl" column="EDUCESSNONRESDTAXCOLL"/>
		<property name="waterTaxColl" column="WATERTAXCOLL"/>
		<property name="fireTaxColl" column="FIRETAXCOLL"/>
		<property name="sewerageTaxColl" column="SEWERAGETAXCOLL"/>
		<property name="lightTaxColl" column="LIGHTTAXCOLL"/>
		<property name="bigBldgTaxColl" column="BIGBLDGTAXCOLL"/>
		<property name="chqBuncPnltyColl" column="CHQBUNCPENALTYCOLL"/>
		<property name="penaltyFineColl" column="PENALTYFINECOLL"/>
		<property name="createdDate" column="CREATEDDATE"/>
	</class>

</hibernate-mapping>
