<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.ptis.domain.entity.property.PropertyReceipt" table="EGPT_PROPERTY_RECEIPTS">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="sequence">
				<param name="sequence">SEQ_EGPTPROPERTY_RECEIPTS</param>
			</generator>
		</id>
		<many-to-one name="basicProperty"
			class="org.egov.ptis.domain.entity.property.BasicPropertyImpl" column="ID_BASIC_PROPERTY"
			insert="false" update="false" />
		
		<property name="bookNumber" column="BOOKNUMBER" type="string"/>
		<property name="receiptNumber" column="RCPT_NUMBER" type="string"/>
		<property name="receiptDate" column="RCPT_DATE" type="java.util.Date"/>
		<property name="fromDate" column="FROM_DATE" type="java.util.Date"/>
		<property name="toDate" column="TO_DATE" type="java.util.Date"/>
		<property name="receiptAmount" column="RCPT_AMOUNT" />
		
 	</class>
	
</hibernate-mapping>