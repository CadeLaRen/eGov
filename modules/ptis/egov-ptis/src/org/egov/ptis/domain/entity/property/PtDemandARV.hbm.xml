<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.ptis.domain.entity.property.PtDemandARV" table="EGPT_PTDEMAND_ARV">
		<id
			name="id" column="ID" type="int">
			<generator class="sequence">
					<param name="sequence">SEQ_DEMANDARV</param>
			</generator>
		</id>
		<timestamp
			column="LASTUPDATEDTIMESTAMP"
			name="lastUpdatedTimeStamp"
			access="property"
			unsaved-value="null"
		/>
		<property name="createTimeStamp" update="false" column="CREATETIMESTAMP"/>
		
		
		<property
			name="arv"
			column="ARV"
		/>
		<property
			name="fromDate"
			column="FROMDATE"
		/>
		<property
			name="type"
			column="ARV_TYPE"
		/>
		<many-to-one name="property" class="org.egov.ptis.domain.entity.property.PropertyImpl" column="ID_PROPERTY" insert="false" update="false" /> 
		<property
			name="toDate"
			column="TODATE"
		/>
		<property
			name="reasonId"
			column="REASONID"
		/>
		<property
			name="section72No"
			column="SECTION72NO"
		/>
		<property
			name="aoNumber"
			column="AONUMBER"
		/>
		<property
			name="aoDate"
			column="AODATE"
		/>
		<property
			name="amalgamatedPid"
			column="AMLGAMATION_PID"
		/>
		<property
			name="netRateOfTax"
			column="NETRATEOFTAX"
		/>
		<property
			name="isHistory"
			column="IS_HISTORY"
		/>
		<property
			name="userId"
			column="USERID"
		/>	
		<property
	        name="notice72Date"
	        column="NOTICE72DATE"
        />	
	</class>
</hibernate-mapping>


