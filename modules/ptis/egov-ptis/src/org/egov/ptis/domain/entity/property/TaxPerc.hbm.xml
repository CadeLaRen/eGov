<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.egov.ptis.domain.entity.property.TaxPerc" table="EGPT_TAX_PERC" >
	<id
			name="id"
			column="ID"
			type="int">
			<generator class="sequence">
				<param name="sequence">SEQ_TAX_PERC</param>
			</generator>
		</id>
		
		<many-to-one name="category" column="ID_CATEGORY" 
			class="org.egov.ptis.domain.entity.property.Category" cascade="all" 
			unique="true"/>  
			
		<many-to-one name="propertyUsage" column="ID_USG_MSTR" 
			class="org.egov.ptis.domain.entity.property.PropertyUsage" cascade="all" 
			unique="true"/>    
		
		<property name="tax_perc" column="TAX_PERC" />
		<property name="fromAmt" column="FROM_AMT" />
		<property name="toAmt" column="TO_AMT" />
		<property name="fromDate" column="FROM_DATE" />
		<property name="toDate" column="TO_DATE" />				
			
    </class>

</hibernate-mapping>
