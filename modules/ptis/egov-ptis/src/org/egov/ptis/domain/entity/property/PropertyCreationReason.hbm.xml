<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.ptis.domain.entity.property.PropertyCreationReason" table="EGPT_PROP_CREATE_REASON">
		<id
			name="idReason" column="ID_REASON" type="int">
			<generator class="sequence">
					<param name="sequence">SEQ_PROP_CREATE_REASON</param>
			</generator>
		</id>
		<timestamp
			column="LASTUPDATEDTIMESTAMP"
			name="lastUpdatedTimeStamp"
			access="property"
			unsaved-value="null"
		/>
		<property
			name="reasonName"
			column="REASON_NAME"
			type="string"
		/>
	</class>
</hibernate-mapping>