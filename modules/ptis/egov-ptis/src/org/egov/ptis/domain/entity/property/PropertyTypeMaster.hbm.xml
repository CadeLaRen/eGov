<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.ptis.domain.entity.property.PropertyTypeMaster"
		table="EGPT_PROPERTY_TYPE_MASTER">
		<id name="id" column="ID_PROPERTYTYPEMASTER" type="java.lang.Long">
			<generator class="sequence">
				<param name="sequence">SEQ_PROPERTYTYPEMASTER</param>
			</generator>
		</id>
		<property name="createdDate" type="java.util.Date">
			<column name="CREATED_DATE" not-null="true" />
		</property>
		<property name="modifiedDate" type="java.util.Date">
			<column name="MODIFIED_DATE" not-null="true" />
		</property>
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl"
			cascade="all">
			<column name="CREATED_BY" />
		</many-to-one>
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl"
			cascade="all">
			<column name="MODIFIED_BY" />
		</many-to-one>
		<property name="type" column="PROPERTY_TYPE" type="string" />

		<property name="factor" column="TYPE_FACTOR" type="float" />
		<property name="code" column="CODE" type="string" />
		<property name="orderNo" column="ORDERNO" type="integer" />

	</class>
</hibernate-mapping>