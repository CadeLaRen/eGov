<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.ptis.domain.entity.property.BoundaryCategory"
		table="EGPT_MSTR_BNDRY_CATEGORY">
		<id name="id" column="ID_BNDRY_CATEGORY" type="java.lang.Long">
			<generator class="sequence">
				<param name="sequence">SEQ_BNDRY_CATEGORY</param>
			</generator>
		</id>
		<property name="createdDate" type="java.util.Date">
			<column name="CREATED_DATE" not-null="true" />
		</property>
		<property name="modifiedDate" type="java.util.Date">
			<column name="MODIFIED_DATE" not-null="true" />
		</property>
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl"
			cascade="all">
			<column name="CREATED_BY" />
		</many-to-one>
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl"
			cascade="all">
			<column name="MODIFIED_BY" />
		</many-to-one>

		<many-to-one name="bndry" column="ID_BNDRY"
			class="org.egov.lib.admbndry.BoundaryImpl" not-null="true" />

		<many-to-one name="category" column="ID_CATEGORY"
			class="org.egov.ptis.domain.entity.property.Category" not-null="true" />

		<property name="fromDate" column="FROM_DATE" />
		<property name="toDate" column="TO_DATE" />

	</class>

</hibernate-mapping>
