<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.works.models.measurementbook.MBBill" table="EGW_MB_BILLREGISTER">
		<id name="id" type="long">
			<column name="ID" />
			<generator class="sequence">
				<param name="sequence">SEQ_EGW_MB_BILL</param>
			</generator>
		</id>
		<many-to-one name="mbHeader"
			class="org.egov.works.models.measurementbook.MBHeader" column="MB_HEADER_ID"
			not-null="true" fetch="select" />
		
		<many-to-one name="egBillregister" class="org.egov.model.bills.EgBillregister" column="BILLREGISTER_ID" not-null="true"  fetch="select" />
	
	</class>
	
</hibernate-mapping>
