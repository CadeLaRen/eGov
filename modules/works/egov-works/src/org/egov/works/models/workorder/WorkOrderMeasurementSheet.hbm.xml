<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.egov.works.models.workorder.WorkOrderMeasurementSheet" table="EGW_WO_MEASUREMENTSHEET">
		<id name="id" column="ID" type="long" unsaved-value="null">
			<generator class="native"><param name="sequence">EGW_WO_MEASUREMENTSHEET_SEQ</param></generator>
		</id>
		<property name="no"><column name="NO"/></property>
		<property name="length"><column name="LENGTH"/></property>
		<property name="width"><column name="WIDTH" /></property>
		<property name="depthOrHeight"><column name="DEPTH_HEIGHT"/></property>
		<property name="quantity"><column name="QUANTITY"/></property>
		<property name="createdDate" column="CREATED_DATE" type="timestamp" />
		<property name="modifiedDate" column="MODIFIED_DATE" type="timestamp" />
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" fetch="select" /> 
       	<many-to-one name="woActivity"
			class="org.egov.works.models.workorder.WorkOrderActivity" fetch="select" insert="false" update="false">
			<column name="WORKORDER_ACTIVITY_ID" not-null="true" />
		</many-to-one> 
		<many-to-one name="measurementSheet"
			class="org.egov.works.models.estimate.MeasurementSheet" >
			<column name="MEASUREMENTSHEET_ID" not-null="true" /> 
		</many-to-one>
	</class>
</hibernate-mapping>
