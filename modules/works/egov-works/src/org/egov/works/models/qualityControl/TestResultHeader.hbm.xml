<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.works.models.qualityControl.TestResultHeader" table="EGW_QC_TESTRESULT_HEADER">
		<id name="id" type="long">
			<column name="ID" />
			<generator class="sequence">
				<param name="sequence">EGW_QC_TESTRESULT_HEADER_SEQ</param>
			</generator>
		</id>

        <many-to-one name="jobHeader" class="org.egov.works.models.qualityControl.JobHeader" column="JOBHEADER_ID" not-null="true" fetch="select"/>
		
        <many-to-one name="egwStatus" column="STATUS_ID" not-null="true" />
        
        <many-to-one name="state" class="org.egov.infstr.models.State" cascade="all">
			<column name="STATE_ID" />
		</many-to-one>
		
        <property name="testResultDate" type="date">
			<column name="TESTRESULT_DATE" length="7" not-null="true" />
		</property>
		
        <list name="testResultDetails" cascade="all,delete-orphan">
			<key column="TESTRESULTHEADER_ID" not-null="true" />
			<list-index column="TESTRESULTDETAILS_INDEX" />
			<one-to-many
				class="org.egov.works.models.qualityControl.TestResultDetails" />
		</list>
		<list name="testResultMis" cascade="all,delete-orphan">
			<key column="TESTRESULTHEADER_ID" not-null="true" />
			<list-index column="TESTRESULTMIS_INDEX" />
			<one-to-many
				class="org.egov.works.models.qualityControl.TestResultMis" />
		</list>
		
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" not-null="true" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" fetch="select"/>
		<property name="createdDate" column="CREATED_DATE" not-null="true" type="timestamp" />
		<property column="MODIFIED_DATE" name="modifiedDate" type="timestamp" />
     </class>
     <query name="getContractorsWithTestResult">
		<![CDATA[
     	select distinct tr.jobHeader.sampleLetterHeader.testSheetHeader.workOrder.contractor from TestResultHeader tr where tr.jobHeader.egwStatus.code!='NEW' order by tr.jobHeader.sampleLetterHeader.testSheetHeader.workOrder.contractor.code 
    ]]>
	</query>
</hibernate-mapping>