<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.works.models.qualityControl.MaterialType" table="EGW_QC_MATERIAL_TYPE">
		<id name="id" type="long">
			<column name="ID" />
			<generator class="sequence">
				<param name="sequence">EGW_QC_MATERIAL_TYPE_SEQ</param>
			</generator>
		</id>
		
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" fetch="select"/>
		<property name="createdDate" column="CREATED_DATE" type="timestamp" />
		<property column="MODIFIED_DATE" name="modifiedDate" type="timestamp" />
		
		<property name="name" type="string">
            <column name="Name" length="256" not-null="true" unique="true"/>
        </property>
		
		<property name="description" type="java.lang.String">
			<column name="DESCRIPTION" length="1024" />
		</property>
		
		<property name="documentNumber" type="long">
           		<column name="DOCUMENT_NUMBER"/>
       	</property>
       	
       	<property name="isActive" type="java.lang.Boolean">
         	<column name="ISACTIVE" precision="1" scale="0" />
		</property>
     </class> 
</hibernate-mapping>		