<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.works.models.estimate.EstimateRateContract" table="EGW_ESTIMATE_RATECONTRACT">
		<id
			name="id"
			column="ID"
			type="long"
			unsaved-value="null">
			<generator class="native">
				<param name="sequence">EGW_ESTIMATE_RATECONTRACT_SEQ</param> 
			</generator>
		</id>
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" fetch="select" />
		<property name="createdDate" column="CREATED_DATE" type="timestamp" />
		<property name="modifiedDate" column="MODIFIED_DATE" type="timestamp" />
       	
       	<many-to-one name="estimate"
			class="org.egov.works.models.estimate.AbstractEstimate" fetch="select" insert="false" update="false" >
			<column name="ESTIMATE_ID" not-null="true"/> 
		</many-to-one>
		
		<many-to-one name="rateContract"
			class="org.egov.works.models.rateContract.RateContract" >
			<column name="RATECONTRACT_ID" not-null="true" /> 
		</many-to-one>
		
		<list name="estimateRCDetailList" cascade="all,delete-orphan" >
			<key column="ESTIMATERATECONTRACT_ID" not-null="true" />
			<list-index column="ESTIMATERCDETAIL_INDEX" />
			<one-to-many class="org.egov.works.models.estimate.EstimateRateContractDetail" />  
		</list>
		
	</class>
</hibernate-mapping>  