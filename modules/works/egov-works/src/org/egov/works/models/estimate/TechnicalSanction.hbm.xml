<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.works.models.estimate.TechnicalSanction" table="EGW_TECHNICALSANCTION_NUMBER">
		<id name="id" type="long">
			<column name="ID" />
			<generator class="sequence">
				<param name="sequence">EGW_TECHSANCTION_NUMBER_SEQ</param>
			</generator>
		</id> 
			
		<many-to-one name="abstractEstimate" class="org.egov.works.models.estimate.AbstractEstimate" column="ABSTRACTESTIMATE_ID" fetch="select"/>
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" fetch="select"/>
		<property name="createdDate" column="CREATED_DATE" type="timestamp" />
		<property column="MODIFIED_DATE" name="modifiedDate" type="timestamp" />
		<property name="techSanctionDate" column="TECHNICALSANCTION_DATE" type="timestamp" />
		<property name="techSanctionNumber" column="TECHNICALSANCTION_NUMBER" type="string" />
	</class>
	<query name="getLatestTechSanctionNumber">
		<![CDATA[
     	from TechnicalSanction as ts where ts.id = ( select max(ts1.id) from TechnicalSanction as ts1 where ts1.abstractEstimate.id=? )
    ]]>
	</query>
</hibernate-mapping> 