<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.works.models.qualityControl.TestRate" table="EGW_QC_TEST_RATE">
		<id name="id" type="long">
			<column name="ID" />
			<generator class="sequence">
				<param name="sequence">EGW_QC_TEST_RATE_SEQ</param>
			</generator>
		</id>
		
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" fetch="select"/>
		<property name="createdDate" column="CREATED_DATE" type="timestamp" />
		<property column="MODIFIED_DATE" name="modifiedDate" type="timestamp" />
		<!-- 
		<property name="rate" type="java.lang.Double">
			<column name="RATE" precision="15"  not-null="true"/>
		</property>
		 -->
		<component name="rate">
			<property name="value" type="double" precision="2"	scale="20" column="RATE" not-null="true"/>
		</component>
		
		<property name="startDate" column="START_DATE" type="timestamp" not-null="true"/>
		<property name="endDate" column="END_DATE" type="timestamp"/>
		
        <many-to-one name="testMaster" class="org.egov.works.models.qualityControl.TestMaster" fetch="select" insert="false" update="false">
            <column name="TESTMASTER_ID" precision="22" scale="0" not-null="true" />
        </many-to-one>
              
     </class> 
</hibernate-mapping>		