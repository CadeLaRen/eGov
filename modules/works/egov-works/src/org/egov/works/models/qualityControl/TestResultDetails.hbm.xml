<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.works.models.qualityControl.TestResultDetails" table="EGW_QC_TESTRESULT_DETAILS">
		<id name="id" type="long">
			<column name="ID" />
			<generator class="sequence">
				<param name="sequence">EGW_QC_TESTRESULT_DETAILS_SEQ</param>
			</generator>
		</id>
        <many-to-one name="testResultHeader" class="org.egov.works.models.qualityControl.TestResultHeader" fetch="select" insert="false" update="false">
            <column name="TESTRESULTHEADER_ID" not-null="true" />
        </many-to-one>
        
	     <many-to-one name="jobDetails" class="org.egov.works.models.qualityControl.JobDetails" column="JOBDETAILS_ID" not-null="true" fetch="join"/>
		
        <property name="remarks"><column name="REMARKS"/></property>
        <property name="documentNumber" type="long"><column name="DOCUMENT_NUMBER" /></property>
        
        <many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" not-null="true" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" fetch="select"/>
		<property name="createdDate" column="CREATED_DATE" not-null="true" type="timestamp" />
		<property column="MODIFIED_DATE" name="modifiedDate" type="timestamp" />
     </class> 
</hibernate-mapping>		