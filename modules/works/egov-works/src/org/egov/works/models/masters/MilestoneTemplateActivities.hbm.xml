<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.egov.works.models.masters.MilestoneTemplateActivity" table="EGW_MILESTONE_TEMP_ACTIVITY " >
  		<id
			name="id"
			column="ID"
			type="long"
			unsaved-value="null">
			<generator class="sequence">
          	<param name="sequence">EGW_MILESTONE_TEM_ACTIVITY_SEQ</param>
            </generator>
        </id>
        <many-to-one name="milestoneTemplate" class="org.egov.works.models.masters.MilestoneTemplate" 
        	fetch="select" insert="false" update="false">
            <column name="TEMPLATEID" precision="22" scale="0" not-null="true"/>
        </many-to-one>
        <property name="description" type="java.lang.String">
            <column name="DESCRIPTION" length="1024" not-null="true" />
        </property>
        <property name="percentage" type="java.math.BigDecimal">
            <column name="PERCENTAGE" precision="22" scale="0" not-null="true" />
        </property>
        <property name="stageOrderNo" type="java.math.BigDecimal">
            <column name="STAGE_ORDER_NO"  precision="5" scale="0" not-null="true" />
        </property>
         <many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATEDBY"/>
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIEDBY"/>
        <property name="modifiedDate" column="MODIFIEDDATE" type="timestamp"/>
		<property name="createdDate" column="CREATEDDATE" />
    </class>
</hibernate-mapping>
