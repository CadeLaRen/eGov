<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.egov.works.models.workorder.AssetsForWorkOrder" table="EGW_ASSETSFORWORKORDER" >
        <id name="id" type="long">
            <column name="ID"/>
            <generator class="sequence" >
            	<param name="sequence">EGW_ASSETSFORWORKORDER_SEQ</param>
            </generator>
        </id>
    	<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" fetch="select"/>
		<property name="createdDate" column="CREATED_DATE" type="timestamp" />
		<property name="modifiedDate" column="MODIFIED_DATE" type="timestamp" />
		<many-to-one name="asset" column="ASSET_ID" not-null="true"/>
		<many-to-one name="workOrderEstimate" column="WORKORDER_ESTIMATE_ID" not-null="true" insert="false" update="false"/>
	</class>
</hibernate-mapping>
