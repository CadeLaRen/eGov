<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.works.models.estimate.EstimateRateContractDetail" table="EGW_ESTIMATE_RC_DETAIL">
		<id
			name="id"
			column="ID"
			type="long"
			unsaved-value="null">
			<generator class="native">
				<param name="sequence">EGW_ESTIMATE_RC_DETAIL_SEQ</param> 
			</generator>
		</id>
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" fetch="select" />
		<property name="createdDate" column="CREATED_DATE" type="timestamp" />
		<property name="modifiedDate" column="MODIFIED_DATE" type="timestamp" />
       	
       	<many-to-one name="activity"
			class="org.egov.works.models.estimate.Activity" >
			<column name="ACTIVITY_ID" not-null="true" /> 
		</many-to-one>
		
		<many-to-one name="estimateRC"
			class="org.egov.works.models.estimate.EstimateRateContract" fetch="select" insert="false" update="false" >
			<column name="ESTIMATERATECONTRACT_ID" not-null="true"/> 
		</many-to-one>
			
	</class>
</hibernate-mapping>  