<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.works.models.qualityControl.TestResultMis" table="EGW_QC_TESTRESULT_MIS">
		<id name="id" type="long">
			<column name="ID" />
			<generator class="sequence">
				<param name="sequence">EGW_QC_TESTRESULT_MIS_SEQ</param>
			</generator>
		</id>
        <many-to-one name="testResultHeader" class="org.egov.works.models.qualityControl.TestResultHeader" fetch="select" insert="false" update="false">
            <column name="TESTRESULTHEADER_ID" not-null="true" />
        </many-to-one>
	     
	     <property name="chequeNumber" type="long">
			<column name="CHEQUE_NUMBER" />
		</property>
		
	     <property name="chequeDate" type="date">
			<column name="CHEQUE_DATE" length="7"  />
		</property>
		
		<many-to-one name="instrumentType" class="org.egov.model.instrument.InstrumentType" column="INSTRUMENTTYPE_ID"/>
		
		<property name="amountReceived" type="double">
            <column name="AMOUNT_RECEIVED" not-null="true" />
        </property>
        
        <property name="remarks"><column name="REMARKS"/></property>
        
        <many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATED_BY" not-null="true" fetch="select" />
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIED_BY" fetch="select"/>
		<property name="createdDate" column="CREATED_DATE" not-null="true" type="timestamp" />
		<property column="MODIFIED_DATE" name="modifiedDate" type="timestamp" />
     </class> 
</hibernate-mapping>		