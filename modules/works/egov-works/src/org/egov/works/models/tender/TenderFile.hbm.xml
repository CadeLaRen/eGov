<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.works.models.tender.TenderFile" table="EGW_TENDERFILE">
		<id name="id" type="long">
			<column name="ID" />
			<generator class="sequence">
				<param name="sequence">EGW_TENDERFILE_SEQ</param>
			</generator>
		</id>
		
		<property name="fileNumber" type="string" >
			<column name="FILENUMBER" length="100" unique="true" />
		</property>
		<property name="fileDate" type="date">
			<column name="FILEDATE" length="7" not-null="true" />
		</property>
		<property name="fileName" type="string">
			<column name="FILENAME" length="500" not-null="true" />
		</property>		
		<property name="fileDescription" type="string">
			<column name="FILEDESCRIPTION" length="1024" />
		</property>	
		<property name="remarks" type="string">
			<column name="REMARKS" length="1024" />
		</property>	
		
		<property name="quotationFlag" type="boolean">
			<column name="isQuotation" />
		</property>
		
		<many-to-one name="department" class="org.egov.lib.rjbac.dept.DepartmentImpl" fetch="select" not-null="true">
			<column name="DEPARTMENT" />
		</many-to-one>
		
		<many-to-one name="egwStatus" column="STATUS_ID" />
		<many-to-one name="state" class="org.egov.infstr.models.State" cascade="all">
			<column name="STATE_ID" />
		</many-to-one>
		<property name="documentNumber" type="long">
			<column name="DOCUMENT_NUMBER" />
		</property>	 
		<many-to-one name="preparedBy" class="org.egov.pims.model.PersonalInformation"> 
			<column name="PREPAREDBY" />
		</many-to-one>
		
		<list name="tenderFileDetails" cascade="all,delete-orphan">
	    	<key column="TENDERFILE_ID"  not-null="true" />
	    	<list-index column="TENDERFILE_INDEX"/>
	    	<one-to-many class="org.egov.works.models.tender.TenderFileDetail" />	
    	</list>
    	
    	<list name="tenderFileNewsPapers" cascade="all,delete-orphan">
	    	<key column="TENDERFILE_ID"  not-null="true" />
	    	<list-index column="TENDERFILE_NEWSPAPER_INDEX"/>
	    	<one-to-many class="org.egov.works.models.tender.TenderFileNewsPapers" />	
    	</list>
    	
    	<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATEDBY" fetch="select" not-null="true"/>
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIEDBY" fetch="select"/>
		<property name="createdDate" column="CREATEDDATE" type="timestamp" not-null="true"/>
		<property name="modifiedDate" column="MODIFIEDDATE" type="timestamp" />		
	</class>
    
</hibernate-mapping>
