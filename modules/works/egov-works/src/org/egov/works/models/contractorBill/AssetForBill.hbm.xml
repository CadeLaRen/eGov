<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.works.models.contractorBill.AssetForBill" table="EGW_ASSETFORBILL">
		<id name="id" type="long">
			<column name="ID" />
			<generator class="sequence">
				<param name="sequence">EGW_ASSETFORBILL_SEQ</param>
			</generator>
		</id>
		<many-to-one name="egbill" class="org.egov.model.bills.EgBillregister" column="ID_BILL" insert="false" update="false"/>
		<many-to-one name="asset" class="org.egov.assets.model.Asset" column="ID_ASSET" fetch="select" />
		<many-to-one name="coa" class="org.egov.commons.CChartOfAccounts" column="ID_COA" fetch="select" />
		<property name="amount">
			<column name="AMOUNT" not-null="true"/>
		</property>
	<property name="description">
			<column name="NARRATION"/>
		</property>
		<many-to-one name="workOrderEstimate" column="WORKORDER_ESTIMATE_ID" not-null="true" fetch="select"/>
	</class>  
</hibernate-mapping>
