<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.pims.model.PayFixedInMaster" table="EGEIS_PAY_FIXED_IN_MSTR">
		
		<id
		name="id"
		column="PAY_FIXED_IN_ID"
		type="int">
		<generator class="sequence">
			<param name="sequence">EGPIMS_PAY_FIXED_SEQ</param>
		</generator>
		</id>
		<property
			name="name"
			column="PAY_FIXED_IN_VALUE"
			
		/>
		
		<property
					name="fromDate"
					column="START_DATE"
					
		/>
		<property
					name="toDate"
					column="END_DATE"
					
		/>	
	</class>
</hibernate-mapping>