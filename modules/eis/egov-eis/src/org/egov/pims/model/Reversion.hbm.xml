<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
   <class name="org.egov.pims.model.Reversion" table="EGEIS_REVERSION" >
        <id name="id" >
            <column name="ID" precision="32" scale="0" />
           <generator class="sequence">
	       	<param name="sequence">EIS_REVERSION_SEQ</param>
    	</generator>
        </id>
        
	<property name="remarks" type="string">
            <column name="REMARKS" length="1024" />
    </property>        
        
    <property name="requestDate" type="date">
         <column name="REQUEST_DATE" length="7" not-null="true" />
    </property>
    
    <property name="reversionEffFrom" type="date">
         <column name="REVERSION_EFF_FROM" length="7" not-null="true" />
    </property>
    
    <property name="docNo" type="long">
            <column name="DOC_NO" />
    </property>
    
    <many-to-one name="assignment" class="org.egov.pims.model.Assignment" fetch="select" cascade="save-update,persist">
		    <column name="ASSIGNMENT_ID" precision="32" scale="0"/>
	</many-to-one>
    
    <many-to-one name="position" class="org.egov.pims.commons.Position" fetch="select">
		    <column name="POSITION_ID" precision="32" scale="0" not-null="true"/>
	</many-to-one>
        
     <many-to-one name="employee" class="org.egov.pims.model.PersonalInformation" fetch="select">
  		 <column name="ID_EMPLOYEE" precision="32" scale="0"/>
 	 </many-to-one>
        
    <many-to-one name="fund" class="org.egov.commons.Fund" fetch="select">
	    <column name="FUND_ID" precision="32" scale="0" />
	</many-to-one>   
	
	<many-to-one name="function" class="org.egov.commons.CFunction" fetch="select">
	    <column name="FUNCTION_ID" precision="32" scale="0" />
	</many-to-one>  
        
    <many-to-one name="desig" class="org.egov.pims.commons.DesignationMaster" fetch="select">
	    <column name="DESIG_ID" precision="32" scale="0" not-null="true"/>
	</many-to-one>
	
        <many-to-one name="dept" class="org.egov.lib.rjbac.dept.DepartmentImpl" fetch="select">
		    <column name="DEPT_ID" precision="32" scale="0" not-null="true" />
	</many-to-one>

	<many-to-one name="status" class="org.egov.commons.EgwStatus" fetch="select">
	   		 <column name="STATUS" precision="12" scale="0" not-null="true" />
    </many-to-one>
	
	<many-to-one name="state" class="org.egov.infstr.models.State" cascade="all">
            <column name="STATE_ID"/>
    </many-to-one>
    
  	<property name="createdDate" type="date">
         <column name="CREATEDDATE" length="7" not-null="true" />
    </property>
    
    <many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" fetch="select" not-null="true">
         <column name="CREATEDBY" precision="12" scale="0"  />
    </many-to-one>
    
    <many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" fetch="select" not-null="true">
        <column name="LASTMODIFIEDBY" precision="12" scale="0"  />
    </many-to-one>
    
    <property name="modifiedDate" column="LASTMODIFIEDDATE" type="timestamp" not-null="true" />
    </class>

</hibernate-mapping>