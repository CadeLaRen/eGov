<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.pims.model.HowAcquiredMaster" table="EGEIS_HOW_ACQUIRED_MSTR">
		<id
							name="id"
							column="HOW_ACQUIRED_ID"
							type="int">
							<generator class="sequence">
								<param name="sequence">EGPIMS_HOW_ACQUIRED_SEQ</param>
							</generator>
		</id>
		<property
			name="name"
			column="HOW_ACQUIRED_VALUE"
			
		/>
		
		<property
					name="fromDate"
					column="START_DATE"
					
		/>
		<property
					name="toDate"
					column="END_DATE"
					
		/>	
	</class>
</hibernate-mapping>