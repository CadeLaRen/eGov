<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.pims.empLeave.model.CalendarYear" table="CALENDARYEAR"> 
		<!-- A squence pk generator -->
		<id name="id" type="long" >
			<column name="ID" />
			<generator class="native">
				<param name="sequence">SEQ_CALENDARYEAR</param>
			</generator>
		</id>	
		<timestamp
					column="LASTMODIFIED"
					name="lastModified"
					access="property"
					unsaved-value="null"/>				
				<property name="calendarYear" >
					<column name="CALENDARYEAR" />
				</property>
				
				<property name="startingDate" >
					<column name="STARTINGDATE" />
				</property>
				<property name="endingDate" >
					<column name="ENDINGDATE" />
				</property>
				<property name="isActive" >
					<column name="ISACTIVE" />
				</property>
				<property name="created" >
					<column name="CREATED" />
				</property>
				
				
				<property name="modifiedBy" >
					<column name="MODIFIEDBY" />
				</property>
				
	</class>
		
</hibernate-mapping>
		