<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.assets.model.AssetOpeningBalance" table="EGASSET_OPENINGBALANCE">
        <id name="id" type="java.lang.Long">
            <column name="ID" precision="22" scale="0" />
            <generator class="sequence">
            	<param name="sequence">seq_egasset_openingbalance</param>
            </generator>
        </id>
        <many-to-one name="asset" class="org.egov.assets.model.Asset" column="ASSETID"/>
        <many-to-one name="financialYear" class="org.egov.commons.CFinancialYear" column="FINANCIALYEARID"/>
        <property name="grossOpeningBalance" type="big_decimal" column="GROSSOPENINGBALANCE"/>
        <property name="deductionOpeningBalance" type="big_decimal" column="DEDUCTIONOPENINGBALANCE"/>
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATEDBY"/>
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="MODIFIEDBY"/>
        <property name="modifiedDate" column="MODIFIEDDATE" type="timestamp"/>
		<property name="createdDate" column="CREATEDDATE" />
    </class>
</hibernate-mapping>