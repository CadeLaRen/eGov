<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-lazy-init="true">
  
  <!-- For Move Boundary Action Injection starts-->
  <bean name="/AdminBoundary/moveBoundary" class="org.egov.infstr.client.adminBoundry.MoveBoundaryAction">
	   <property name="moveBoundaryDelegate" ref="moveBoundaryDelegate"/>
  </bean>
  <!-- For Move Boundary Action Injection ends-->

  <bean name="/admin/CreateModule" class="org.egov.infstr.client.administration.rjbac.module.CreateModuleAction"> 
       <property name="genericCommonService" ref="genericCommonService"/>
  </bean>
  
  <bean name="/admin/BeforeUpdateModule" class="org.egov.infstr.client.administration.rjbac.module.BeforeUpdateModuleAction"> 
       <property name="genericCommonService" ref="genericCommonService"/>
  </bean>
 
  <bean name="/admin/UpdateModule" class="org.egov.infstr.client.administration.rjbac.module.UpdateModuleAction"> 
       <property name="genericCommonService" ref="genericCommonService"/>
  </bean>
  
  <bean name="/admin/BeforeCreateOrUpdateAction" class="org.egov.infstr.client.administration.rjbac.action.BeforeCreateOrUpdateAction"> 
       <property name="genericCommonService" ref="genericCommonService"/>
  </bean>
 
  <bean name="/admin/CreateOrUpdateAction" class="org.egov.infstr.client.administration.rjbac.action.CreateOrUpdateAction"> 
       <property name="genericCommonService" ref="genericCommonService"/>
  	   <property name="rbacManager" ref="rbacMgr"/>
  </bean>
  
  <bean name="/admin/BeforeViewRoleModule" class="org.egov.infstr.client.administration.rjbac.action.BeforeViewRoleModule"> 
       <property name="roleService" ref="roleService"/>
  </bean>
  
  <bean name="/admin/BeforeCreateOrUpdateRoleAction" class="org.egov.infstr.client.administration.rjbac.action.BeforeCreateOrUpdateRoleAction"> 
       <property name="genericCommonService" ref="genericCommonService"/>
       <property name="roleService" ref="roleService"/>
  </bean>
  
  <bean name="/admin/CreateOrUpdateRoleAction" class="org.egov.infstr.client.administration.rjbac.action.CreateOrUpdateRoleAction"> 
       <property name="rbacService" ref="rbacService"/>
       <property name="roleService" ref="roleService"/>
  </bean>  
  
  <bean name="/admin/chgPassword" class="org.egov.infstr.client.administration.rjbac.user.ChangePasswordAction"> 
       <property name="userDao" ref="userDao"/>
  </bean>

  <bean name="/config/MasterSetUpAction" class="org.egov.infstr.config.client.MasterSetUpAction"> 
  </bean>
  <bean name="/SetupUser" class="org.egov.infstr.client.administration.rjbac.user.SetupUserAction">
  	<property name="userDelegate" ref="userDelegate"/>
  </bean>
</beans>
