<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.commons.CVoucherHeader" table="VOUCHERHEADER"> 
		<id name="id" type="long" unsaved-value="null" >
			<column name="ID" not-null="true"/>
			<generator class="native">
				<param name="sequence">SEQ_VOUCHERHEADER</param>
			</generator>
		</id>	
		<property name="cgn">
			<column name="CGN"/>
		</property>	
		<property name="cgDate">
			<column name="CGDATE"/>
		</property>	
		<property name="name">
			<column name="NAME"/>
		</property>	
		<property name="type">
			<column name="TYPE"/>
		</property>	
		<property name="description">
			<column name="DESCRIPTION"/>
		</property>	
		<property name="effectiveDate">
			<column name="EFFECTIVEDATE" not-null="true"/>
		</property>	
		<property name="voucherNumber">
			<column name="VOUCHERNUMBER"/>
		</property>	
		<property name="voucherDate" type="date">
			<column name="VOUCHERDATE"/>
		</property>	
		<property name="departmentId">
			<column name="DEPARTMENTID"/>
		</property>	
		<many-to-one name="fundId" class="org.egov.commons.Fund" column="FUNDID" />
		<property name="fiscalPeriodId">
			<column name="FISCALPERIODID"/>
		</property>	
		<property name="status">
			<column name="STATUS"/>
		</property>	
		<property name="originalvcId">
			<column name="ORIGINALVCID"/>
		</property>	
		<many-to-one name="fundsourceId" class="org.egov.commons.Fundsource" column="FUNDSOURCEID" />
		<property name="isConfirmed">
			<column name="ISCONFIRMED"/>
		</property>	
		<many-to-one name="createdBy" class="org.egov.lib.rjbac.user.UserImpl" column="CREATEDBY"/>
		<property name="functionId">
			<column name="FUNCTIONID"/>
		</property>	
		<property name="refcgNo">
			<column name="REFCGNO"/>
		</property>	
		<property name="cgvn">
			<column name="CGVN"/>
		</property>	
		<many-to-one name="modifiedBy" class="org.egov.lib.rjbac.user.UserImpl" column="LASTMODIFIEDBY"/>
		<property name="modifiedDate" column="LASTMODIFIEDDATE"/>
		<property name="moduleId">
			<column name="MODULEID"/>
		</property>
		<many-to-one name="state" class="org.egov.infstr.models.State" cascade="all">
            <column name="STATE_ID"/>
        </many-to-one>	
        <set name="voucherDetail" cascade="save-update,delete,delete-orphan" inverse="true">
            <key>
                <column name="VOUCHERHEADERID" precision="22" scale="0"/>
            </key>
            <one-to-many class="org.egov.commons.VoucherDetail" />
        </set> 
        <one-to-one name="vouchermis" class="org.egov.commons.Vouchermis" property-ref="voucherheaderid" cascade="all" /> 
	</class>
</hibernate-mapping>