<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.lib.citizen.model.Owner" table="EG_CITIZEN">
		<id name="citizenID" column="CITIZENID" type="int">
			<generator class="native">
				<param name="sequence">SEQ_CITIZEN</param>
			</generator>
		</id>
		<timestamp column="LASTUPDATEDTIMESTAMP" name="lastUpdatedTimeStamp" access="property" unsaved-value="null" />
		<property name="createdAt" type="java.sql.Timestamp" update="true" insert="true" access="property">
			<column name="CREATIONTIMESTAMP" />
		</property>
		<property name="createdBy" update="true" insert="true" access="property">
			<column name="USERID" />
		</property>
		<property name="lastModifiedBy" update="true" insert="true" access="property">
			<column name="UPDATEUSERID" />
		</property>
		<property name="ssn" column="SSN" />
		<property name="panNumber" column="PANNUMBER" />
		<property name="passportNumber" column="PASSPORTNUMBER" />
		<property name="drivingLicenceNumber" column="DRIVINGLICENCENUMBER" />
		<property name="rationCardNumber" column="RATIONCARDNUMBER" />
		<property name="voterRegistrationNumber" column="VOTERREGISTRATIONNUMBER" />
		<property name="firstName" column="FIRSTNAME" />
		<property name="middleName" column="MIDDLENAME" />
		<property name="lastName" column="LASTNAME" />
		<property name="birthDate" column="BIRTHDATE" type="java.util.Date" />
		<property name="homePhone" column="HOMEPHONE" />
		<property name="officePhone" column="OFFICEPHONE" />
		<property name="mobilePhone" column="MOBILEPHONE" />
		<property name="fax" column="FAX" />
		<property name="emailAddress" column="EMAILADDRESS" />
		<property name="occupation" column="OCCUPATION" />
		<property name="jobStatus" column="JOBSTATUS" />
		<property name="locale" column="LOCALE" />
		<property name="firstNameLocal" column="FIRSTNAMELOCAL" />
		<property name="middleNameLocal" column="MIDDLENAMELOCAL" />
		<property name="lastNameLocal" column="LASTNAMELOCAL" />
		<property name="ownerTitle" column="OWNERTITLE" />
		<property name="ownertitleLocal" column="OWNERTITLELOCAL" />
		<property name="sex" column="SEX" />
		<property name="fatherName" column="FATHERNAME" />
		<!-- Mapping the Link table btn Owner and its Multiple addresses -->
		<set name="addressSet" cascade="all" table="EGPT_CITIZEN_ADDRESS">
			<key column="ID_CITIZEN" not-null="true" />
			<many-to-many class="org.egov.lib.address.model.Address" column="ID_ADDRESS" />
		</set>
	</class>
</hibernate-mapping>