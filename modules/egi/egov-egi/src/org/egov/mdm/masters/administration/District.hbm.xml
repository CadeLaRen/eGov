<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.mdm.masters.administration.District" table="EG_DISTRICT"
		dynamic-update="false" dynamic-insert="false" select-before-update="false"
		optimistic-lock="version">
		<id name="id" type="java.lang.Integer">
			<column name="DISTRICTID" />
			<generator class="native"/>
		</id>
		<property name="name" type="java.lang.String" update="true" insert="true" access="property">
			<column name="DISTRICTNAME" />
		</property>
		<property name="districtConst" type="java.lang.String" update="true" insert="true" access="property">
			<column name="DISTRICTCONST" />
		</property>
		<property name="districtNameLocal" type="java.lang.String" update="true" insert="true" access="property">
			<column name="DISTRICTNAMELOCAL" />
		</property>
		<property name="stateId" type="java.lang.Integer" update="true" insert="true" access="property">
			<column name="STATEID" />
		</property>
	</class>
</hibernate-mapping>
